ton.Backdrop  = p		  end if		  		  if tempTheme.editRacers_Button <> Nil and tempTheme.editRacers_Mask <> Nil and tempTheme.editRacers_MouseOver <> Nil then		    p = New Picture(tempTheme.editRacers_Button.Width,tempTheme.editRacers_Button.Height,32)		    p.Graphics.DrawPicture tempTheme.editRacers_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.editRacers_Mask,0,0		    Canvas_ERButton.Backdrop  = p		  end if		  		  if tempTheme.statistics_Button <> Nil and tempTheme.statistics_Mask <> Nil and tempTheme.statistics_MouseOver <> Nil then		    p = New Picture(tempTheme.statistics_Button.Width,tempTheme.statistics_Button.Height,32)		    p.Graphics.DrawPicture tempTheme.statistics_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.statistics_Mask,0,0		    Canvas_StatButton.Backdrop  = p		  end if		  		  if tempTheme.navigation_Button <> Nil and tempTheme.navigation_Mask <> Nil and tempTheme.navigation_MouseOver <> Nil then		    p = New Picture(tempTheme.navigation_Button.Width,tempTheme.navigation_Button.Height,32)		    p.Graphics.DrawPicture tempTheme.navigation_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.navigation_Mask,0,0		    Canvas_NavigationButton.Backdrop  = p		  end if		  		  if tempTheme.scaleBackground = true then		    CheckBox_ScaleBackground.Value = true		  else		    CheckBox_ScaleBackground.Value = false		  end if		  		  if tempTheme.countdown0 <> Nil then		    Canvas_Countdown0.Backdrop = scalePicture(tempTheme.countdown0,Canvas_Countdown0.Width, Canvas_Countdown0.Height)		  end if		  if tempTheme.countdown1 <> Nil then		    Canvas_Countdown1.Backdrop = scalePicture(tempTheme.countdown1,Canvas_Countdown1.Width, Canvas_Countdown1.Height)		  end if		  if tempTheme.countdown2 <> Nil then		    Canvas_Countdown2.Backdrop = scalePicture(tempTheme.countdown2,Canvas_Countdown2.Width, Canvas_Countdown2.Height)		  end if		  if tempTheme.countdown3 <> Nil then		    Canvas_Countdown3.Backdrop = scalePicture(tempTheme.countdown3,Canvas_Countdown3.Width, Canvas_Countdown3.Height)		  end if		  if tempTheme.countdown4 <> Nil then		    Canvas_Countdown4.Backdrop = scalePicture(tempTheme.countdown4,Canvas_Countdown4.Width, Canvas_Countdown4.Height)		  end if		  if tempTheme.countdown5 <> Nil then		    Canvas_Countdown5.Backdrop = scalePicture(tempTheme.countdown5,Canvas_Countdown5.Width, Canvas_Countdown5.Height)		  end if		  if tempTheme.countdown10 <> Nil then		    Canvas_Countdown10.Backdrop = scalePicture(tempTheme.countdown10,Canvas_Countdown10.Width, Canvas_Countdown10.Height)		  end if		  		  if tempTheme.gettingStarted_Button <> Nil and tempTheme.gettingStarted_Mask <> Nil then		    p = New Picture(tempTheme.gettingStarted_Button.Width,tempTheme.gettingStarted_Mask.Height,32)		    p.Graphics.DrawPicture tempTheme.gettingStarted_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.gettingStarted_Mask,0,0		    Canvas_GettingStarted.Backdrop  = scalePicture(p,Canvas_GettingStarted.Width, Canvas_GettingStarted.Height)		  end if		  if tempTheme.setUp_Button <> Nil and tempTheme.setUp_Mask <> Nil then		    p = New Picture(tempTheme.setUp_Button.Width,tempTheme.setUp_Button.Height,32)		    p.Graphics.DrawPicture tempTheme.setUp_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.setUp_Mask,0,0		    Canvas_SetUp.Backdrop  = scalePicture(p,Canvas_SetUp.Width, Canvas_SetUp.Height)		  end if		  if tempTheme.startARace_Button <> Nil and tempTheme.startARace_Mask <> Nil then		    p = New Picture(tempTheme.startARace_Button.Width,tempTheme.startARace_Button.Height,32)		    p.Graphics.DrawPicture tempTheme.startARace_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.startARace_Mask,0,0		    Canvas_StartARace.Backdrop  = scalePicture(p,Canvas_StartARace.Width, Canvas_StartARace.Height)		  end if		  if tempTheme.advancedTopics_Button <> Nil and tempTheme.advancedTopics_Mask <> Nil then		    p = New Picture(tempTheme.advancedTopics_Button.Width,tempTheme.advancedTopics_Button.Height,32)		    p.Graphics.DrawPicture tempTheme.advancedTopics_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.advancedTopics_Mask,0,0		    Canvas_AdvancedTopics.Backdrop  = scalePicture(p,Canvas_AdvancedTopics.Width, Canvas_AdvancedTopics.Height)		  end if		  if tempTheme.topic_Button <> Nil and tempTheme.topic_Mask <> Nil then		    p = New Picture(tempTheme.topic_Button.Width,tempTheme.topic_Mask.Height,32)		    p.Graphics.DrawPicture tempTheme.topic_Button,0,0		    p.Mask.Graphics.DrawPicture tempTheme.topic_Mask,0,0		    Canvas_Topic.Backdrop  = p		  end if		  		End Sub	#tag EndMethod	#tag Property, Flags = &h21		Private imageMask As Picture	#tag EndProperty	#tag Property, Flags = &h21		Private imagePicture As Picture	#tag EndProperty	#tag Property, Flags = &h0		saving As Boolean = false	#tag EndProperty	#tag Property, Flags = &h21		Private soundBinary As sound	#tag EndProperty	#tag Property, Flags = &h21		Private soundFolderItem As FolderItem	#tag EndProperty	#tag Property, Flags = &h21		Private themeButton As picture	#tag EndProperty	#tag Property, Flags = &h21		Private themeButtonMask As Picture	#tag EndProperty	#tag Property, Flags = &h21		Private themeButtonMouseOver As Picture	#tag EndProperty#tag EndWindowCode#tag Events PushButton_New	#tag Event		Sub Action()		  dim status as boolean		  dim themes() as string		  saving = false		  //Create a new theme name based on what is entered into the combobox		  //If nothing was previously selected then load the default theme settings		  		  if PopupMenu_ThemeSelect.Text = "" then		    MsgBox t_pleaseenteruniquethemename		    return		  end if		  		  themes = tempTheme.listThemes		  		  for each theme as string in themes		    if PopupMenu_ThemeSelect.Text = theme then		      MsgBox t_pleaseenteruniquethemename		      return		    end if		  next		  		  dim tempWindow as new messageWindow		  tempWindow.message = t_creating		  tempWindow.timeLimit = 100		  tempWindow.Show		  		  if tempTheme.themeName = "" then		    tempTheme.set("Flip Side")		    updateScreen		  end if		  		  status = tempTheme.create(PopupMenu_ThemeSelect.Text)		  		  if status = true then		    //We succeeded in creating a new theme, we should add it to the popup menu for the main window		    mainWindow.PopupMenu1_GC_SelectTheme.AddRow(PopupMenu_ThemeSelect.Text)		    PopupMenu_ThemeSelect.AddRow(PopupMenu_ThemeSelect.Text)		  end if		  		  tempWindow.timeOut		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_Delete	#tag Event		Sub Action()		  tempTheme.delete(PopupMenu_ThemeSelect.Text, false)		  saving = false		  //Rebuild the pop up menu		  PopupMenu_ThemeSelect.DeleteAllRows		  populateThemePopup		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_Import	#tag Event		Sub Action()		  dim success as string		  dim tempWindow as new messageWindow		  tempWindow.message = t_importing		  tempWindow.timeLimit = 100		  tempWindow.Show		  		  saving = true		  		  		  success = tempTheme.import		  		  if success = "" then		    //Nothing was imported		    tempWindow.timeOut		    tempWindow.Close		    MsgBox "Error, theme was not imported"		    		  else		    // We created a new theme		    populateThemePopup		    PopupMenu_ThemeSelect.Text = success		    tempTheme.set(success)		    		    updateScreen		    		    mainWindow.PopupMenu1_GC_SelectTheme.AddRow(PopupMenu_ThemeSelect.Text)		    		    tempWindow.timeOut		  end if		  		  if success = mainWindow.fsTheme.themeName then		    mainWindow.fsTheme.set(mainWindow.fsTheme.themeName)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_Export	#tag Event		Sub Action()		  dim tempWindow as new messageWindow		  saving = false		  tempWindow.message = t_exporting		  tempWindow.timeLimit = 100		  tempWindow.Show		  		  tempTheme.export(PopupMenu_ThemeSelect.Text)		  		  tempWindow.timeOut		  tempWindow.Close		End Sub	#tag EndEvent#tag EndEvents#tag Events PopupMenu_ThemeSelect	#tag Event		Sub Open()		  dim s() as string		  		  s = tempTheme.listThemes		  		  if UBound(s) > -1 then		    for each element as string in s		      me.AddRow(element)		      if element = mainWindow.fsTheme.themeName then		        me.ListIndex = me.ListCount - 1		      end if		    next		  end if		End Sub	#tag EndEvent	#tag Event		Sub Change()		  load(me.Text)		End Sub	#tag EndEvent#tag EndEvents#tag Events tempTheme	#tag Event		Sub themeDeleted(theme as string)		  if saving = false then		    if theme = G_Preferences.readS("themeName") then		      //We just deleted the theme that was loaded		      //Set the theme to the default		      G_Preferences.write("themeName", "Flip Side")		      		      //Change to this row in the popup menu		      for i as integer = 0 to mainWindow.PopupMenu1_GC_SelectTheme.ListCount - 1		        if mainWindow.PopupMenu1_GC_SelectTheme.List(i) = "default" or mainWindow.PopupMenu1_GC_SelectTheme.List(i) = "Flip Side" then		          //We found a match, engage		          mainWindow.PopupMenu1_GC_SelectTheme.ListIndex = i		        end if		        		      next		      		    end if		    		    //Remove the option from the popup menu		    for i as integer = 0 to mainWindow.PopupMenu1_GC_SelectTheme.ListCount - 1		      if mainWindow.PopupMenu1_GC_SelectTheme.List(i) = theme then		        //We found a match, destroy		        mainWindow.PopupMenu1_GC_SelectTheme.RemoveRow(i)		      end if		      		    next		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_Save	#tag Event		Sub Action()		  //Umm, save?		  saving = true		  //Pop up a message saying we saved		  dim tempWindow as new messageWindow		  tempWindow.message = t_saving		  tempWindow.timeLimit = 100		  tempWindow.Show		  Save		  tempWindow.timeOut		End Sub	#tag EndEvent#tag EndEvents#tag Events Listbox_ItemColors	#tag Event		Function CellTextPaint(g As Graphics, row As Integer, column As Integer, x as Integer, y as Integer) As Boolean		  //Find what row is being painted and check the theme for what the color should be		  dim c as color		  		  select case row		    		  case 0		    c = tempTheme.backgroundColor		  case 1		    c = tempTheme.defaultText		  case 2		    c = tempTheme.mainScreenText		  case 3		    c = tempTheme.raceScreenText		  case 4		    c = tempTheme.generalConfigScreenText		  case 5		    c = tempTheme.statisticsScreenText		  case 6		    c = tempTheme.raceConfigScreenText1		  case 7		    c = tempTheme.raceConfigScreenText2		  case 8		    c = tempTheme.raceConfigScreenText3		  case 9		    c = tempTheme.editRacerScreenText1		  case 10		    c = tempTheme.editRacerScreenText2		  case 11		    c = tempTheme.navigationText		  case 12		    c = tempTheme.topicText		  end select		  		  if c = &cFFFFFF then		    g.ForeColor = &c000000		    g.FillRect(0,0,g.Width,g.Height)		  end if		  		  g.ForeColor = c		End Function	#tag EndEvent	#tag Event		Function CellClick(row as Integer, column as Integer, x as Integer, y as Integer) As Boolean		  //Find what row was selected, prompt for a color, and then save that color to the correct theme setting		  Dim c  as Color		  Dim b as Boolean		  		  		  b = SelectColor(c,"Select a Color")		  if b=true then		    		    //Figure out which row we are one to find what to update		    select case row		      		    case 0		      tempTheme.backgroundColor = c		    case 1		      tempTheme.defaultText = c		    case 2		      tempTheme.mainScreenText = c		    case 3		      tempTheme.raceScreenText = c		    case 4		      tempTheme.generalConfigScreenText = c		    case 5		      tempTheme.statisticsScreenText = c		    case 6		      tempTheme.raceConfigScreenText1 = c		    case 7		      tempTheme.raceConfigScreenText2 = c		    case 8		      tempTheme.raceCon