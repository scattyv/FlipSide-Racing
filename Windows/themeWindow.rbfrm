empTheme.themeDB = G_Database		  		End Sub	#tag EndEvent	#tag Method, Flags = &h21		Private Sub load(name as String)		  if name <> "" then		    dim tempWindow as new messageWindow		    tempWindow.message = t_loading		    tempWindow.timeLimit = 100		    tempWindow.Show		    		    		    tempTheme.set(name)		    		    updateScreen		    		    tempWindow.timeOut		    		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub populateThemePopup()		  dim themeNames() as string		  		  themeNames = tempTheme.listThemes		  		  PopupMenu_ThemeSelect.DeleteAllRows		  		  if UBound(themeNames) > -1 then		    for each s as string in themeNames		      PopupMenu_ThemeSelect.AddRow(s)		    next		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub save()		  tempTheme.save(PopupMenu_ThemeSelect.Text)		  		  //Check to see if this is the current theme		  if PopupMenu_ThemeSelect.Text = "" then		    MsgBox t_pleaseselectthemetoedit		    		  elseif PopupMenu_ThemeSelect.Text = mainWindow.fsTheme.themeName then		    //If it is then we need to set it so it will refresh		    mainWindow.fsTheme.set(mainWindow.fsTheme.themeName)		  end if		  saving = false		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Function scalePicture(sourcepic as picture, destwidth as integer, destheight as integer) As picture		  dim newHeight as integer		  dim newWidth as integer		  dim oldRatio as double		  dim newRatio as double		  dim newerPicture as picture		  dim tempRatio as double		  dim tempPicture as Picture		  dim tempPic as picture		  tempPicture = sourcepic		  		  //Make sure that the resized width and height are valid		  if destwidth < 1 or destwidth > 32767 or destheight < 1 or destheight > 32767 then		    //We can not resize to those dimensions		    dim emptyPic as new picture(1, 1, 32)		    return emptyPic		  end if		  		  //Make sure we got a picture		  if sourcepic = Nil then		    //We did not get a picture		    dim emptyPic as new picture(destwidth, destheight, 32)		    return emptyPic		  end if		  		  //Make sure the source pic has valid dimensions		  if sourcepic.Width < 1 or sourcepic.Width > 32767 or sourcepic.Height < 1 or sourcepic.Height > 32767 then		    //We did not get a usable picture		    dim emptyPic as new picture(destwidth, destheight, 32)		    return emptyPic		  end if		  		  oldRatio = destwidth/destheight		  newRatio = tempPicture.Width/tempPicture.Height		  if oldRatio < newRatio then		    //New image ratio is wider		    //Fix width to 100 and calculate what the height should be		    newWidth = destwidth		    tempRatio = tempPicture.Width/newWidth		    newHeight = tempPicture.Height/tempRatio		    		  else		    //New image is taller		    //Fix height to 40 and calculate what the width should be		    newHeight = destheight		    tempRatio = tempPicture.Height/newHeight		    newWidth = tempPicture.Width/tempRatio		    		  end if		  newerPicture=New Picture(newWidth,newHeight,32)		  		  newerPicture.Graphics.DrawPicture(tempPicture,0,0,newWidth,newHeight,0,0,tempPicture.Width,tempPicture.Height)		  tempPic = newerPicture		  		  return tempPic		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Sub selectBackgroundImage()		  Dim dlg as OpenDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  dim tempPic as Picture		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New OpenDialog		  dlg.Title=t_selectanimagefile		  dlg.InitialDirectory = w.Child("theme")		  f=dlg.ShowModal()		  		  		  if f<>nil then		    tempTheme.backgroundImage = Picture.Open(f)		    tempPic = Picture.Open(f)		    if tempTheme.backgroundImage = Nil then		      MsgBox t_selectanimagefile		      return		    end if		    		    //Add ability to resize image to display on smaller canvas		    if tempTheme.backgroundImage.Height > 110 or tempTheme.backgroundImage.Width > 176 then		      dim newHeight as integer		      dim newWidth as integer		      dim oldRatio as double		      dim newRatio as double		      dim newerPicture as picture		      dim tempRatio as double		      dim tempPicture as Picture		      tempPicture = tempPic		      		      oldRatio = 176/110		      newRatio = tempPicture.Width/tempPicture.Height		      if oldRatio < newRatio then		        //New image ratio is wider		        //Fix width to 100 and calculate what the height should be		        newWidth = 176		        tempRatio = tempPicture.Width/newWidth		        newHeight = tempPicture.Height/tempRatio		        		      else		        //New image is taller		        //Fix height to 40 and calculate what the width should be		        newHeight = 100		        tempRatio = tempPicture.Height/newHeight		        newWidth = tempPicture.Width/tempRatio		        		      end if		      newerPicture=New Picture(newWidth,newHeight,32)		      		      newerPicture.Graphics.DrawPicture(tempPicture,0,0,newWidth,newHeight,0,0,tempPicture.Width,tempPicture.Height)		      tempPic = newerPicture		      		    end if		    		    Canvas_Background.Backdrop = tempPic		    		    		  else		    //They did not select an image, must have hit cancel instead		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Function selectButtonImages(buttonName as String, buttonCanvas as Canvas) As boolean		  //This method will prompt with a series of 3 dialog boxes to select button images		  		  Dim dlg as OpenDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  dim fMask as FolderItem		  dim fMouseOver as FolderItem		  dim success as boolean		  		  success = false		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New OpenDialog		  dlg.Title="Select a " + buttonName + " button image file"		  dlg.InitialDirectory = w.Child("theme")		  f=dlg.ShowModal()		  		  //Make sure an image was selected otherwise return		  if f = Nil then		    Return success		  end if		  		  		  dlg= New OpenDialog		  dlg.Title="Select a " + buttonName + " button mask file"		  dlg.InitialDirectory = f		  fMask=dlg.ShowModal()		  		  //Make sure an image was selected otherwise return		  if fMask = Nil then		    Return success		  end if		  		  dlg= New OpenDialog		  dlg.Title="Select a " + buttonName + " button mouse over image file"		  dlg.InitialDirectory = fMask		  fMouseOver=dlg.ShowModal()		  		  if f <> Nil and fMask <> Nil and fMouseOver <> Nil then		    //We can use what we were given, we hope		    //Set the themes pictures		    themeButton = Picture.Open(f)		    themeButtonMask = Picture.Open(fMask)		    themeButtonMouseOver = Picture.Open(fMouseOver)		    		    //Build image to display on canvas		    dim p as Picture = New Picture(themeButton.Width,themeButton.Height,32)		    p.Graphics.DrawPicture Picture.Open(f),0,0		    p.Mask.Graphics.DrawPicture Picture.Open(fMask),0,0		    		    buttonCanvas.Backdrop = p		    		    //Set the themes pictures		    themeButton = Picture.Open(f)		    themeButtonMask = Picture.Open(fMask)		    themeButtonMouseOver = Picture.Open(fMouseOver)		    		    success = true		    		  else		    //failed		    success = false		  end if		  		  Return success		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function selectImage(imageName as String, imageCanvas as Canvas) As Boolean		  //This method will prompt with a series of 3 dialog boxes to select button images		  		  Dim dlg as OpenDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  dim success as boolean		  success = false		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New OpenDialog		  dlg.Title="Select a " + imageName + " image file"		  dlg.InitialDirectory = w.Child("theme")		  f=dlg.ShowModal()		  		  //Make sure an image was selected otherwise return		  if f = Nil then		    Return success		  end if		  		  if f <> Nil then		    //We can use what we were given, we hope		    		    //Set the themes pictures		    imagePicture = Picture.Open(f)		    		    //Build image to display on canvas		    if imagePicture <> Nil then		      dim p as Picture = New Picture(imagePicture.width,imagePicture.height,32)		      p.Graphics.DrawPicture Picture.Open(f),0,0		      		      imageCanvas.Backdrop = scalePicture(p,imageCanvas.Width, imageCanvas.Height)		      		      success = true		    else		      success = false		    end if		    		  else		    success = false		    		  end if		  		  Return success		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Function selectImageAndMask(imageName as String, imageCanvas as Canvas) As Boolean		  //This method will prompt with a series of 3 dialog boxes to select button images		  		  Dim dlg as OpenDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  dim fMask as FolderItem		  dim success as boolean		  success = false		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New OpenDialog		  dlg.Title="Select a " + imageName + " image file"		  dlg.InitialDirectory = w.Child("theme")		  f=dlg.ShowModal()		  		  //Make sure an image was selected otherwise return		  if f = Nil then		    Return success		  end if		  		  dlg= New OpenDialog		  dlg.Title="Select a " + imageName + " mask file"		  dlg.InitialDirectory = f		  fMask=dlg.ShowModal()		  		  		  if f <> Nil and fMask <> Nil then		    //We can use what we were given, we hope		    		    //Set the themes pictures		    imagePicture = Picture.Open(f)		    imageMask = Picture.Open(fMask)		    		    if imagePicture = Nil or imageMask = Nil then		      return success		    end if		    		    //Build image to display on canvas		    dim p as Picture = New Picture(imagePicture.width,imagePicture.height,32)		    p.Graphics.DrawPicture Picture.Open(f),0,0		    p.Mask.Graphics.DrawPicture Picture.Open(fMask),0,0		    		    //imageCanvas.Backdrop = scalePicture(p,imageCanvas.Width, imageCanvas.Height)		    imageCanvas.Backdrop = p		    		    success = true		    		  else		    success = false		    		  end if		  		  Return success		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Sub selectMainLogo()		  dim success as boolean		  		  success = selectImageAndMask(t_mainlogo, Canvas_MainLogo)		  		  //Check to see if we have anything to do		  if success = true then		    tempTheme.mainLogo = imagePicture		    tempTheme.mainLogo_Mask = imageMask		    dim tempPic as picture		    		    tempPic = tempTheme.mainLogo		    //Resize picture to fit		    if tempTheme.mainLogo.Height > 63 or tempTheme.mainLogo.Width > 174 then		      dim newHeight as integer		      dim newWidth as integer		      dim oldRatio as double		      dim newRatio as double		      dim newerPicture as picture		      dim tempRatio as double		      dim tempPicture as Picture		      dim tempMask as Picture		      		      tempPicture = tempTheme.mainLogo		      tempMask = tempTheme.mainLogo_Mask		      		      oldRatio = 174/63		      newRatio = tempPicture.Width/tempPicture.Height		      if oldRatio < newRatio then		        //New image ratio is wider		        //Fix width to 100 and calculate what the height should be		        newWidth = 174		        tempRatio = tempPicture.Width/newWidth		        newHeight = tempPicture.Height/tempRatio		        		      else		        //New image is taller		        //Fix height to 40 and calculate what the width should be		        newHeight = 63		        tempRatio = tempPicture.Height/newHeight		        newWidth = tempPicture.Width/tempRatio		        		      end if		      newerPicture=New Picture(newWidth,newHeight,32)		      		      newerPicture.Graphics.DrawPicture(tempPicture,0,0,newWidth,newHeight,0,0,tempPicture.Width,tempPicture.Height)		      newerPicture.Mask.Graphics.DrawPicture(tempMask,0,0,newWidth,newHeight,0,0,tempPicture.Width,tempPicture.Height)		      tempPic = newerPicture		      		      		    end if		    Canvas_MainLogo.Backdrop = tempPic		    		    		    		  else		    MsgBox t_pleaseselectimageandmask		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Function selectSound(soundName as string) As boolean		  //Use this method for selecting and saving the sound files into the theme		  		  Dim dlg as OpenDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  		  dim success as boolean		  success = false		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New OpenDialog		  dlg.Title="Select the " + soundName + " sound file"		  dlg.InitialDirectory = w.Child("theme")		  f=dlg.ShowModal()		  		  if f<> Nil then		    //they selected a file so we can save it to the theme		    soundFolderItem = f		    soundBinary = f.OpenAsSound		    		    success = true		    		  else		    		    //No file selected, nothing to do		    success = false		  end if		  		  Return success		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Sub updateScreen()		  //use this method to update the screen with the currently selected theme data		  		  //Update text color control		  Listbox_ItemColors.InvalidateCell(-1,-1)		  		  //Update scaling checkbox		  //CheckBox_ScaleBackground.Value = tempTheme.scaleBackground		  		  //Update images		  dim p as picture		  if tempTheme.backgroundImage <> Nil then		    Canvas_Background.Backdrop = scalePicture(tempTheme.backgroundImage,Canvas_Background.Width, Canvas_Background.Height)		  end if		  		  if tempTheme.emptyRacer <> Nil and tempTheme.emptyRacer_Mask <> Nil then		    p = New Picture(tempTheme.emptyRacer.width,tempTheme.emptyRacer.Height,32)		    p.Graphics.DrawPicture tempTheme.emptyRacer,0,0		    p.Mask.Graphics.DrawPicture 