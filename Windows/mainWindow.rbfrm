ankText(i).Text = "0"		    mainWindow.BestLapRankText(i).TextColor = &cBABABA		    mainWindow.BestLapRankText(i).Visible = False		    		    mainWindow.PageRankText(i).Text="0"//pace, need to make name pace later		    mainWindow.PageRankText(i).TextColor = &cBABABA		    mainWindow.PageRankText(i).Visible = False		  next		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function convertToHex(data as string) As string		  dim hexPart as string		  dim hexString as string		  		  for i as Integer=1 to lenb(data)		    hexPart = hex(ascb(midb(data,i,1)))		    if lenb(hexPart)<2 then		      hexpart="0"+hexPart		    end if		    if i<>lenb(data) then		      hexString=hexString+hexPart+" "		    else		      hexString=hexString+hexPart//so we don't get extra space at end throwing off command size		    end if		  next		  		  return hexString		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Sub CopyRacer(racerid as String)		  dim newracerid as string		  //Copy the racer		  newracerid = G_Preferences.theRaceObject.CopyRacer(racerid)		  		  //Check to see if we got a new racer id back		  if newracerid <> "" then		    //Refresh the screen		    PopulateUIDComboBox		    PopulateRacerListBox()		    		    //Select whoever was just added		    for i as integer=0 to RacerListBox.ListCount-1		      if RacerListBox.cell(i,0)=newracerid then		        RacerListBox.Selected(i)=true		      else		        RacerListBox.Selected(i)=false//deselect old selections		      end if		    next		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub CreateRacer()		  //if your not already on the correct page panel lets get there		  PagePanel1.Value=3		  dim rs as RecordSet		  rs = G_Database.SQLSelect("SELECT image FROM racer_table WHERE name==''")		  if rs.RecordCount > 0 then		    //We already have created this		  else		    //Create a new racer		    G_Database.SQLExecute("INSERT INTO racer_table (name,uid,scale,cha