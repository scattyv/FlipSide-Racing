.Enabled=false		  ListBox_ST_Racers.Enabled=false		  ListBox_ST_IndividualRacerTable.Enabled=false		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub emailRaceResults(event_name as string, redo as Boolean, override as Boolean)		  //Check to see if we are supposed to do anything		  if G_Preferences.readI("emailRecipients") <> 0 then		    dim subject as string		    dim event_id as string		    event_id = getEventID(event_name)		    dim rs as RecordSet		    dim racers() as string		    dim admins() as string		    dim raceremails() as string		    dim body as string		    dim imagepath as string		    dim racetype as string		    		    //Generate the body		    body = G_Preferences.theRaceObject.GenerateHTMLReport(event_name, true)		    		    //Generate the subject		    if redo = True then		      subject = G_Preferences.readS("clubName") + ", " + t_correction + ": " + event_name		    else		      subject = G_Preferences.readS("clubName") + ", " + event_name		    end if		    		    //Generate the image path		    imagepath = GetFolderItem("club_logo").Child(G_Preferences.clubLogoPath).AbsolutePath		    		    //Get the admins		    admins = splitb(G_Preferences.readS("emailAdmins"),",")		    		    //Lets find the race type		    rs = G_Database.SQLSelect("SELECT race_type FROM race_table WHERE event_id=='"+event_id+"'")		    if rs <> nil then		      racetype = rs.Field("race_type").StringValue		    end if		    		    		    //Figure out what to do with the race type		    //But of override is true then we can skip this as we will email no matter what		    if override = False then		      select case G_Preferences.readI("emailRaceReports")		      case 0		        if racetype <> "Main" then		          return		        end if		        		      case 1		        //Nothing to do, we will email all types		        		      case 2		        if racetype = "Practice" then		          return		        end if		        		      end select		    end if		    //Lets find the racers in the race		    rs = G_Database.SQLSelect("SELECT racer_id FROM summary_table WHERE event_id=='"+event_id+"'")		    //Check to see if we got any records if so get the id's		    if rs <> nil then		      while not rs.EOF		        racers.Append(rs.Field("racer_id").StringValue)		        rs.MoveNext		      wend		    end if		    		    //now if we have racer id's then we need to get there emails		    if UBound(racers) > -1 then		      for each racer as string in racers		        		        rs = G_Database.SQLSelect("SELECT email FROM racer_table WHERE racer_id=='"+racer+"'")		        //Check to see if we got anything		        if rs <> nil then		          while not rs.EOF		            //Make sure it is  not an  empty or null record		            if rs.Field("email").StringValue <> "" and rs.Field("email").StringValue <> ChrB(&h00) then		              raceremails.Append(rs.Field("email").StringValue)		            end if		            rs.MoveNext		          wend		        end if		        		        		      next		    end if		    		    //Now send emails to admins		    if G_Preferences.readI("emailRecipients") = 1 or G_Preferences.readI("emailRecipients") = 3 then		      if UBound(admins) > -1 then		        for each admin as string in admins		          emailThread1.newMessage(subject,body, "", admin)		        next		      end if		      		    end if		    		    //Now send email to racers		    if G_Preferences.readI("emailRecipients") = 2 or G_Preferences.readI("emailRecipients") = 3 then		      if UBound(raceremails) > -1 then		        for each raceremail as string in raceremails		          emailThread1.newMessage(subject,body, "", raceremail)		        next		      end if		    end if		    		    //Check to see if there are messages in the queue, if so send		    if UBound(emailThread1.emailMessages) > -1 then		      emailThread1.Run		    end if		    		    		  end if		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub EnableInRaceControls()		  startStopButton.Enabled=true		  startRaceButton.Enabled=true		  		  		  		  //raceTypePopUpBox.Enabled=true		  //lapsField.Enabled=true		  //addToLapsButton.Enabled=true		  //removeFromLapsButton.Enabled=true		  //timeLimitField.Enabled=true		  //addToTimeLimitButton.Enabled=true		  //removeFromTimeLimitButton.Enabled=true		  //minLapTimeField.Enabled=true		  //addToMinLapTimeButton.Enabled=true		  //removeFromMinLapTimeButton.Enabled=true		  //countDownTimeField.Enabled=true		  //addToCountDownTimeButton.Enabled=true		  //removeFromCountDownTimeButton.Enabled=true		  //roundTypePopUpMenu.Enabled=true		  ////selectRacersButton.Enabled=true		  createNewRacerButton.Enabled=true		  scanForUIDButton.Enabled=true		  saveButton.Enabled=true		  deleteButton.Enabled=true		  browseForNewImageButton.Enabled=true		  		  stopRaceButton.Enabled=false		  PushButton_ST_Merge.Enabled=true		  PushButton_ST_Split.Enabled=true		  //mainWindow.MenuBarVisible = true		  		  //Enable Control Tabs		  editRacersCButton.Enabled=true		  editRacersCButton.Backdrop=editRacers_Button		  generalConfigCButton.Enabled=true		  generalConfigCButton.Backdrop=generalConfig_Button		  raceConfigCButton.Enabled=true		  raceConfigCButton.Backdrop=raceConfig_Button		  		  //Since you might want to allow adding in racers mid game, the selectRacersCButton will stay 'active', but you can definently not 'remove' a racer.		  		  //Statistics Screen controls that need disabled		  BevelButton_ST_ExportHFAYOLPS.Enabled=true		  BevelButton_ST_ViewReport.Enabled=true		  BevelButton_ST_EmailReport.Enabled=true		  BevelButton_ST_DeleteEvent.Enabled=true		  BevelButton_ST_DeleteRacer.Enabled=true		  BevelButton_ST_ReassignRacer.Enabled=true		  PushButton_ST_Merge.Enabled=true		  PushButton_ST_Split.Enabled=true		  ListBox_ST_Events.Enabled=true		  ListBox_ST_Racers.Enabled=true		  ListBox_ST_IndividualRacerTable.Enabled=true		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub EndTheRace()		  static processing as boolean = false		  		  //Make sure there is a race to end		  //if M_RaceActive = false then		  //exit		  //end if		  		  //Since this routine can be called from multiple locations we want to prevent possible double processing		  if processing = false then		    processing = true		    M_RaceEnding= false//not ending anymore		    M_RaceActive = false//because the race isn't active anymore		    raceTimer.Mode=0		    countDownTimer.Mode=0		    trackUIDtimer.Mode=0		    G_Preferences.theRaceObject.state="stopped"		    startStopButton.Caption=t_startrace		    startStopButton.Enabled = true  //For some hardware types this button was disabled during race		    timeRemainingText = t_finished		    Thread_Core166.Kill		    EnableInRaceControls()		    //Force a refresh of the screen		    AnimateRaceScreen		    me.Refresh		    //Turn off all timer controls and restore control to buttons and controls.		    dim racerinrace as boolean		    racerinrace = false		    		    //Refresh the time remaining graphic		    DB_TimeRemaining.Refresh		    		    //Clear the serial buffer just in case		    Select Case G_Preferences.hardwareType		    Case "CoreSpeedway","CoreSpeedway i166"		      redim assembledHex(-1)		      serial1.Write(ChrB(&h01) + ChrB(&h0A) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&hF4) + ChrB(&h00) + ChrB(&hFF) + ChrB(&h00))		      SerialLapCounters2.initialize		      Serial1.XmitWait		      Serial1.Flush		    Case "AMB20"		      SerialLapCounters1.Flush		    end select		    		    		    //We don't want to delete everything here, just the timeSinceLastChecked in case the user wants to view graphs and 'save' the data later~		    dim i as Integer		    dim k as Integer		    dim activeIndexes(-1) as integer = FindActiveIndexes()		    for i=0 to UBound(activeIndexes)		      M_Slots(activeIndexes(i)).timeSinceLastCheck=0//this way it knows to reset the calculations if you add the user.  (In raceObject->CalculateNewRacerValues)		      M_Slots(activeIndexes(i)).tempTimeSinceLastCheck=0		      M_Slots(activeIndexes(i)).raceEnded=false//defaults		      M_Slots(activeIndexes(i)).EndingRace=false//defaults		    next		    G_Preferences.theRaceObject.timeElapsed=0//reset these		    G_Preferences.theRaceObject.timeRemaining=0		    G_Preferences.theRaceObject.tempTimeElapsed=0		    G_Preferences.theRaceObject.tempTimeLimit=0		    M_StartTime=0		    		    		    if G_Preferences.theRaceObject.RaceType <> "Free Play" then		      //Make sure we are not in free play mode before storing results		      dim query as string		      dim event_ID as string = str(G_Preferences.theRaceObject.currentEventID)		      		      //Summary Table Information to be added~		      dim rank as string		      dim avg as string		      dim best_time as string		      dim best_lap as string		      dim laps_leading as string		      dim total_laps as string		      dim final_time as string		      //This first check prevents software from trying to insert records when no racers were added.		      if UBound(activeIndexes) > -1 then		        for i = 0 to UBound(activeIndexes)//CrashBug#40_definentely knows even if you added a racer that's 'active' so that is not problem		          //Check racer to make sure they actually did something		          if ubound(M_Slots(activeIndexes(i)).lapTimes) >= 0 then		            racerinrace = true		            rank = str(M_Slots(activeIndexes(i)).currentPlace)		            avg = format(M_Slots(activeIndexes(i)).average, G_Preferences.lapTimeFormat)		            avg = str(CDbl(avg))		            best_time = str(M_Slots(activeIndexes(i)).best_time)		            best_lap = str(M_Slots(activeIndexes(i)).best_Lap)//might be problem in query?		            laps_leading = str(M_Slots(activeIndexes(i)).lapsLeading)		            total_laps = str(M_Slots(activeIndexes(i)).lapCount)		            final_time = str(M_Slots(activeIndexes(i)).lapTimes(UBound(M_Slots(activeIndexes(i)).lapTimes)))		            query = query + _		            "INSERT INTO summary_table(racer_id,event_id,rank,average_time,best_time,best_lap,laps_leading,total_laps,final_time) VALUES ('"+str(M_Slots(activeIndexes(i)).RacerID)+"','"+event_ID+"','"+rank+"','"+avg+"','"+best_time+"','"+best_lap+"','"+laps_leading+"','"+total_laps+"','"+final_time+"');"		          else		            //the racer did nothing so we need to delete them out of the summary table and the linking table		            query = query + "DELETE FROM summary_table WHERE racer_id = " + str(M_Slots(activeIndexes(i)).RacerID) + " AND event_id = " + event_ID + ";"		            //query = query + "DELETE FROM race_racer_table WHERE racer_id = " + str(M_Slots(activeIndexes(i)).RacerID) + " AND event_id = " + event_ID + ";"		          end if		        next		        		      end if		      		      //Checks to see if at least 1 racer was in the race if not get rid of the tables		      if racerinrace = false then		        query = query + "DELETE FROM race_table WHERE event_id = " + event_ID + ";"		        query = query + "DELETE FROM summary_table WHERE event_id = " + event_ID + ";"		      end if		      		      //Check to make sure we have a query to run		      if query = "" then		        query = "DELETE FROM race_table WHERE event_id = " + event_ID + ";"		        query = "DELETE FROM summary_table WHERE event_id = " + event_ID + ";"		        G_Database.SQLExecute(query)		        		        		        if G_Database.Error=true then		          MsgBox "Error inserting racers data to summary_table"+EndOfLine+EndOfLine+G_Database.ErrorMessage+EndOfLine+query		        else		          G_Database.Commit()//save changes~		        end if		        		      else		        		        G_Database.SQLExecute(query)		        		        		        if G_Database.Error=true then		          MsgBox "Error inserting racers data to summary_table"+EndOfLine+EndOfLine+G_Database.ErrorMessage+EndOfLine+query		        else		          G_Database.Commit()//save changes~		        end if		        		        //Laps Table Information to be added~		        query=""//reset it since I'm using the same variable again		        		        //This check makes sure that at least 1 racer was added to the race		        if UBound(activeIndexes) > -1 then		          for i=0 to UBound(activeIndexes)		            //Check to make sure racer actually completed a lap, if not do not insert their record		            if ubound(M_Slots(activeIndexes(i)).lapTimes) >= 0 then		              		              for k=0 to UBound(M_Slots(activeIndexes(i)).lapCounts)//Lap Counts array will match the others, so no need for another inner loop.		                query=query+_		                "INSERT INTO laps_table(racer_id,event_id,lap_number,lap_time,lag_time,time_at_lap,lap_place) VALUES ('"+_		                str(M_Slots(activeIndexes(i)).RacerID)+"','"+_		                event_ID+"','"+_		                str(M_Slots(activeIndexes(i)).lapCounts(k))+"','"+_		                str(M_Slots(activeIndexes(i)).lapTimes(k))+"','"+_		                str(M_Slots(activeIndexes(i)).lagTimes(k))+"','"+_		                str(M_Slots(activeIndexes(i)).timeAtLap(k))+"','"+_		                str(M_Slots(activeIndexes(i)).lapPlaces(k))+"');"		              next		              		            end if		          next		        end if		        		        		        //Check to make sure we have a query to run		        //This gets rid of the error that can occur if a race is ended that has no racers		        if query <> "" then		          		          //insert data into laps table		          		          G_Database.SQLExecute(query)		          		          query = ""		          		          if G_Database.Error= true then		            MsgBox "Error inserting racers data to laps_table"+EndOfLine+EndOfLine+G_Database.ErrorMessage		          else		            G_Database.Commit()		          end if		          		          //Insert the lap information into a backup table		          //Build each insert separately instead of doing a SQL SELECT from the laps table to prevent possible duplicate keys		          for i=0 to UBound(activeIndexes)		            //Check to make sure racer actually completed a lap, if not do not insert their record		            if ubound(M_Slots(activeIndexes(i)).lapTimes) >= 0 then		              		              for k=0 to UBound(M_Slots(activeIndexes(i)).lapCounts)//Lap Counts array will match the others, so no need for another inner loop.		                query=query+_		                "INSERT INTO laps_backup_table(racer_id,event_id,lap_number,lap_time,lag_time,time_at_lap,lap_place) VALUES ('"+_		                str(M_Slots(activeIndexes(i)).RacerID)+"','"+_		                event_ID+"','"+_		                str(M_Slots(activeIndexes(i)).lapCounts(k))+"','"+_		                str(M_Slots(activeIndexes(i)).lapTimes(k))+"','"+_		                str(M_Slots(activeIndexes(i)).lagTimes(k))+"','"+_		                str(M_Slots(activeIndexes(i)).timeAtLap(k))+"','"+_		                str(M_Slots(activeIndexes(i)).lapPlaces(k))+"');"		              next		              		            end if		          next		          		          G_Database.SQLExecute(query)		          		          if G_Database.Error=true then		            MsgBox "Error inserting racers data to laps_backup_table"+EndOfLine+EndOfLine+G_Database.ErrorMessage		          else		            G_Database.Commit()//save changes~		          end if		        end if		        		        //Now update the events, so we can see the new event added to the table!		        ShowAllEvents()		        		        //The code below will make sure that it selects the race you just did so it still shows up in the statistics screen even after the race ended!		        for i=0 to ListBox_ST_Events.ListCount-1		          if ListBox_ST_Events.cell(i,0)=G_Preferences.theRaceObject.eventName then		            ListBox_ST_Events.Selected(i)=true		          end if		        next		        ListBox_ST_Events.ColumnSortDirection(1) = ListBox.SortDescending		        ListBox_ST_Events.SortedColumn=1		        ListBox_ST_Events.Sort		      end if		      		      //Turn on relayboard light if enabled		      if G_Preferences.relayPort <> "None" then		        relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		        relayBoard.enablePort(G_Preferences.relayPort, 2, "b")		        relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		        relayBoard.enablePort(G_Preferences.relayPort, 4, "a")		      end if		      SerialLapCounters1.lightState("stop")		      		      //Play end of race sound		      if G_Preferences.useAudio=true then		        robotSpeak(G_Preferences.speechText.Value("racecomplete"), true)		      end if		      		      //Email results if needed		      if racerinrace = True then		        emailRaceResults(G_Preferences.theRaceObject.eventName, false, false)		      end if		      		    end if		    		    //Speak the final rank if we are not in free play mode		    if G_Preferences.theRaceObject.RaceType <> "Free Play" then		      robotSpeak(G_Preferences.theRaceObject.RankString, false)		    end if		    		    //If the race is managed we need to link it to the event		    if G_Preferences.theRaceObject.managed = True then		      fsEventManager.linkRace(G_Preferences.theRaceObject.currentEventID, G_Preferences.theRaceObject.eventItem, G_Preferences.theRaceObject.eventName)		    end if		    		    //Done processing		    processing = false		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub export_HFAYOLPSformat()		  dim event_id as string = getEventID(WhatEventIsSelected())		  dim racer_ids() as string = getRacerIDs(event_id)		  		  dim rs as RecordSet		  dim query as string		  		  Dim dlg as SaveAsDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New SaveAsDialog		  dlg.Title=t_saveyourhfayexport		  if TargetLinux = true then		    dlg.InitialDirectory = SpecialFolder.UserHome		  else		    dlg.InitialDirectory = SpecialFolder.Desktop		  end if		  dlg.SuggestedFileName = "HFAY_"+G_Preferences.readS("clubName")+"_"+Replaceall(WhatEventIsSelected,":","-")+".txt"		  f=dlg.ShowModal()		  		  		  //dim f as FolderItem = GetSaveFolderItem("","HFAY_"+G_Preferences.readS("clubName")+".txt")		  dim filestream as TextOutputStream		  		  dim time as double		  dim minutes as Integer		  dim seconds as Double		  dim timeString as string		  dim formattedseconds as string		  dim aname() as string		  dim amaxlap() as string		  dim atime() as string		  dim aclub() as string		  dim achassis() as string		  dim amotor() as string		  dim aaverage() as double		  		  		  if f<>nil then		    filestream = TextOutputStream.Create(f)		    for i as Integer=0 to UBound(racer_ids)		      query = "SELECT racer_table.name, racer_table.chassis, racer_table.motor, max(laps_table.lap_number) as max_lap, max(laps_table.lap_time) as max_time FROM racer_table,laps_table WHERE racer_table.racer_id=='"+racer_ids(i)+"' AND laps_table.racer_id=='"+racer_ids(i)+_		      "' AND laps_table.event_id=='"+event_id+"';"		      		      rs = G_Database.SQLSelect(query)		      if rs<>nil then		        while not rs.EOF		          time = rs.Field("max_time").DoubleValue		          timeString = Format(time,"0.00")		          time = cdbl(timeString)		          minutes = time/60		          seconds = ((time * 100) mod 6000) /100		          formattedseconds = Format(seconds,"00.00")		          formattedseconds = replace(formattedseconds, ",",".")		          aname.Append(rs.Field("name").StringValue)		          amaxlap.Append(rs.Field("max_lap").StringValue)		          atime.Append(Format(minutes,"###0")+":"+formattedseconds)		          aclub.Append(G_Preferences.readS("clubName"))		          achassis.Append(rs.Field("chassis").StringValue)		          amotor.Append(rs.Field("motor").StringValue)		          //amotor.Append("")		          aaverage.Append(rs.Field("max_time").DoubleValue/rs.Field("max_lap").IntegerValue)		          		          //filestream.Writeline rs.Field("name").StringValue+","+_		          //rs.Field("max_lap").StringValue+","+_		          //Format(minutes,"###0")+":"+formattedseconds+","+_		          //G_Preferences.clubName+","+_		          //rs.Field("chassis").StringValue+","+_		          //rs.Field("motor").StringValue		          		          rs.MoveNext		        wend		        		        		      else		        MsgBox "Failed to export, error in query."+EndOfLine+EndOfLine+G_Database.ErrorMessage+EndOfLine+EndOfLine+query		      end if		    next		    		    if UBound(aaverage) > -1 then		      //Sort and write the data		      aaverage.SortWith(aname, amaxlap, atime, aclub, achassis, amotor)		      		      for k as integer = 0 to UBound(aname)		        		        filestream.Writeline aname(k)+","+_		        amaxlap(k)+","+_		        atime(k)+","+_		        aclub(k)+","+_		        achassis(k)+","+_		        amotor(k)		        		      next		    end if		    		    		    		    filestream.Close		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function FindActiveIndexes() As integer()		  dim i as Integer		  dim activeIndexes(-1) as Integer		  		  for i=0 to 49//find the active indexes		    if M_Slots(i)<>nil then		      if M_Slots(i).Active=true then		        activeIndexes.Append i		      end if		    end if		  next		  		  return activeIndexes		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function getEventID(event_name as string) As string		  dim rs as RecordSet		  rs = G_Database.SQLSelect("SELECT * FROM race_table WHERE event_name=='"+event_name+"'")		  		  if rs<>nil then		    return rs.Field("event_id").StringValue		  else		    MsgBox "Error, no such event in race_table"		  end if		  		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function getRacerIDs(event_id as string) As string()		  dim rs as RecordSet		  dim racer_ids(-1) as string		  		  rs = G_Database.SQLSelect("SELECT * FROM summary_table WHERE event_id=='"+event_id+"'")		  		  if rs<>nil then		    while not rs.EOF		      racer_ids.Append rs.Field("racer_id").StringValue		      rs.MoveNext		    wend		    return racer_ids		  else		    MsgBox "Error, no records returned in summary_table"		  end if		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function getRacerName(racer_id as string) As string		  dim rs as RecordSet		  dim name as string		  rs = G_Database.SQLSelect("SELECT * FROM racer_table WHERE racer_id=='"+racer_id+"'")		  if rs<>nil then		    name=rs.Field("name").StringValue		    return name		  else		    MsgBox "Error finding racer name"		  end if		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Sub graphOptionsChanged()		  'dim pass as Boolean		  'dim row as Integer		  ''		  'if graphType = "Laps Versus Time" then		  'M_graphTypeSelected=graphType//sets the local global variable so other windows know what to kind of graph to draw if they wanted.		  'if M_RaceActive=true then		  'GM_AnimateStatisticsCanvas(G_Preferences.theRaceObject.activeRacers,M_graphTypeSelected,"Time (seconds)","Laps",statisticsCanvas,"Time_Graph")		  'else		  'selectEventWindow.Show		  'if selectEventWindow.ListBox_ST_Events.ListCount<1 then		  'selectEventWindow.ShowAllEvents()//displays from the database the known events and the dates they happened.		  'end if		  'selectEventWindow.ShowRacerData(selectEventWindow.WhatEventIsSelected)		  'end if		  'elseif graphType = "Laps Versus Lag" then		  'M_graphTypeSelected=graphType		  'if M_Rac