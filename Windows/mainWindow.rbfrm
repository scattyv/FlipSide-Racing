bject.lapIndication="Lap Time 1 Decimal"		    StaticText_RC_BestLapAnnounce.Enabled = True		    Popupmenu_RC_BestLapAnnounce.Enabled = True		  case 4		    G_Preferences.theRaceObject.lapIndication="Lap Difference 1 Decimal"		    StaticText_RC_BestLapAnnounce.Enabled = False		    Popupmenu_RC_BestLapAnnounce.Enabled = False		    Popupmenu_RC_BestLapAnnounce.ListIndex = 0		  case 5		    G_Preferences.theRaceObject.lapIndication="Lap Time 2 Decimals"		    StaticText_RC_BestLapAnnounce.Enabled = True		    Popupmenu_RC_BestLapAnnounce.Enabled = True		  case 6		    G_Preferences.theRaceObject.lapIndication="Lap Difference 2 Decimals"		    StaticText_RC_BestLapAnnounce.Enabled = False		    Popupmenu_RC_BestLapAnnounce.Enabled = False		    Popupmenu_RC_BestLapAnnounce.ListIndex = 0		  end select		  		  if G_Preferences.theRaceObject.RaceType = "Drag" then		    StaticText_RC_BestLapAnnounce.Enabled = False		    Popupmenu_RC_BestLapAnnounce.Enabled = False		  end if		  		  		  if windowOpened = True then		    G_Preferences.write("lapIndication",G_Preferences.theRaceObject.lapIndication)//save changes~		  end if		  		End Sub	#tag EndEvent#tag EndEvents#tag Events lapsField	#tag Event		Sub TextChange()		  		  if IsNumeric(me.Text)=false and me.text<>"" then		    me.Text="1"		    me.SelStart=1//right of the '1'		    G_Preferences.theRaceObject.numLaps=cdbl(me.Text)		  elseif me.Text="" then		    G_Preferences.theRaceObject.numLaps=1		  elseif me.Text="0" then//can't have '0' laps		    me.Text="1"		    me.SelStart=1//right of the '1'		    G_Preferences.theRaceObject.numLaps=1		  else//then it's normal		    G_Preferences.theRaceObject.numLaps=cdbl(me.Text)		  end if		  if windowOpened = true then		    G_Preferences.write("numLaps",G_Preferences.theRaceObject.numLaps)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events addToLapsButton	#tag Event		Sub Action()		  dim laps as Integer = cdbl(lapsField.Text)		  		  lapsF