 32)		    return emptyPic		  end if		  		  //Make sure we got a picture		  if sourcepic = Nil then		    //We did not get a picture		    dim emptyPic as new picture(destwidth, destheight, 32)		    return emptyPic		  end if		  		  //Make sure the source pic has valid dimensions		  if sourcepic.Width < 1 or sourcepic.Width > 32767 or sourcepic.Height < 1 or sourcepic.Height > 32767 then		    //We did not get a usable picture		    dim emptyPic as new picture(destwidth, destheight, 32)		    return emptyPic		  end if		  		  		  if fixedratio = true then		    //Maintain the aspect ratio of the picture		    oldRatio = destwidth/destheight		    newRatio = tempPicture.Width/tempPicture.Height		    if oldRatio < newRatio then		      //New image ratio is wider		      newWidth = destwidth		      tempRatio = tempPicture.Width/newWidth		      newHeight = tempPicture.Height/tempRatio		      		    else		      //New image is taller		      newHeight = destheight		      tempRatio = tempPicture.Height/newHeight		      newWidth = tempPicture.Width/tempRatio		      		    end if		    newerPicture=New Picture(newWidth,newHeight,32)		    		  else		    newWidth=destwidth		    newHeight=destheight		    newerPicture=New Picture(destwidth,destheight,32)		    		  end if		  		  		  newerPicture.Graphics.DrawPicture(tempPicture,0,0,newWidth,newHeight,0,0,tempPicture.Width,tempPicture.Height)		  tempPic = newerPicture		  		  return tempPic		End Function	#tag EndMethod	#tag Method, Flags = &h0		Sub ScanButtonsVisible(scanvisible as boolean)		  If scanvisible = True then		    		    		    scanForUIDButton.Visible = True		    editRacerScanProgressBar.Visible = False		  else		    		    		    scanForUIDButton.Visible = False		    editRacerScanProgressBar.Visible = False		  end if		  		  if G_Preferences.hardwareType <> "Keyboard" and G_Preferences.hardwareType <> "CoreSpeedway" and G_Preferences.hardwareType <> "CoreSpeedway 166" then		    scanForUIDButton.Visible = true		    editRacerScanProgressBar.Visible = true		  end if		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function secondsToTime(seconds as double, secondsFormat as string) As string		  dim d as new date		  dim t as string		  dim i as integer		  dim h as double		  dim s as double		  		  d.totalseconds = seconds		  		  if secondsFormat = "0.000" then		    //Get thousandths value		    i = floor(seconds)		    h = (seconds - i) * 1000		    		    s = d.second + h/1000		    		    secondsFormat = "00.000"		  else		    // Get the hundreths value to add to strings		    i = floor(seconds)		    h = (seconds - i) * 100		    		    s = d.second + h/100		    		    secondsFormat = "00.00"		  end if		  		  select case d.TotalSeconds		    		  case is >= 86400		    //Time is a day or greater		    t  = format(d.Day-1, "##") + ":" + format(d.hour, "00") + ":" + format(d.minute, "00") + ":" + format(s, secondsFormat)		    		  case is >= 3600		    //Time is a hour or greater		    t  = format(d.hour, "##") + ":" + format(d.minute, "00") + ":" + format(s, secondsFormat)		    		  case is >= 60		    //Time is a minute or greater		    t  = format(d.minute, "##") + ":" + format(s, secondsFormat)		  else		    //Time is less than a minute		    if secondsFormat = "00.000" then		      t = format(s, "##.000")		    else		      t = format(s, "##.00")		    end if		  end select		  		  return t		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Sub SelectPopUpMenuOption(thePopUpMenu as popupmenu, theString as string)		  //Code to select correct PopupMenu option		  dim i as Integer		  dim indexL as integer		  		  indexL = 0		  		  if thePopUpMenu.ListCount = 0 then		    //There are no items in the popup menu to select		    return		  end if		  		  for i=0 to thePopUpMenu.ListCount-1		    if thePopUpMenu.List(i)=theString then		      indexL = i		      exit		    end if		  next		  		  thePopUpMenu.ListIndex = indexL		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub SetCarImage()		  Dim dlg as OpenDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New OpenDialog		  dlg.Title="Select an image file"		  dlg.InitialDirectory = w.Child("car_images")		  f=dlg.ShowModal()		  		  dim fMask as FolderItem		  		  if f<>nil then		    M_tempCarImage = Picture.Open(f)		    M_tempCarImagePath = f.GetSaveInfo(g, 0)		    if M_tempCarImage<>nil then		      //Add ability to resize image		      if M_tempCarImage.Height > 40 or M_tempCarImage.Width > 100 then		        dim newHeight as integer		        dim newWidth as integer		        dim oldRatio as double		        dim newRatio as double		        dim newerPicture as picture		        dim tempRatio as double		        dim tempPicture as Picture		        tempPicture = M_tempCarImage		        		        oldRatio = 100/40		        newRatio = tempPicture.Width/tempPicture.Height		        if oldRatio < newRatio then		          //New image ratio is wider		          //Fix width to 100 and calculate what the height should be		          newWidth = 100		          tempRatio = tempPicture.Width/newWidth		          newHeight = tempPicture.Height/tempRatio		          		        else		          //New image is taller		          //Fix height to 40 and calculate what the width should be		          newHeight = 40		          tempRatio = tempPicture.Height/newHeight		          newWidth = tempPicture.Width/tempRatio		          		        end if		        newerPicture=New Picture(newWidth,newHeight,32)		        		        newerPicture.Graphics.DrawPicture(tempPicture,0,0,newWidth,newHeight,0,0,tempPicture.Width,tempPicture.Height)		        M_tempCarImage = newerPicture		        		      end if		      		      carCanvas.Backdrop = M_tempCarImage		      		      //Fixme, removing selection of image mask as it is confusing for users		      //need a new way of doing this		      //dlg= New OpenDialog		      //dlg.Title="Select a mask for the image file.  If none, press cancel."		      //dlg.InitialDirectory = f		      //fMask=dlg.ShowModal()		      		      if fMask<>nil and fMask.Exists=true then		        M_tempCarMask = Picture.Open(fMask)//stores the mask		        M_tempCarMaskPath = fMask.GetSaveInfo(g, 0)		        //resize if needed		        		        if M_tempCarMask.Height > 40 or M_tempCarMask.Width > 100 then		          dim newHeight as integer		          dim newWidth as integer		          dim newerPicture as picture		          dim oldRatio as double		          dim newRatio as double		          dim tempRatio as double		          dim tempPicture as Picture		          		          oldRatio = 100/40		          newRatio = tempPicture.Width/tempPicture.Height		          if oldRatio < newRatio then		            //New image ratio is wider		            //Fix width to 100 and calculate what the height should be		            newWidth = 100		            tempRatio = tempPicture.Width/newWidth		            newHeight = tempPicture.Height/tempRatio		            		          else		            //New image is taller		            //Fix height to 40 and calculate what the width should be		            newHeight = 40		            tempRatio = tempPicture.Height/newHeight		            newWidth = tempPicture.Width/tempRatio		            		          end if		          newerPicture = New Picture(newWidth,newHeight,32)		          		          newerPicture.Graphics.DrawPicture(tempPicture,0,0,newWidth,newHeight,0,0,tempPicture.Width,tempPicture.Height)		          M_tempCarMask = newerPicture		          		        end if		        		        		        dim p as Picture = New Picture(M_tempCarImage.Width,M_tempCarImage.Height,32)//must do it this way because otherwise it changes the image by reference and it causes all kinds of problems.		        p.Graphics.DrawPicture M_tempCarImage,0,0		        p.Mask.Graphics.DrawPicture M_tempCarMask,0,0		        		        //M_tempCarImage.Mask.Graphics.DrawPicture M_tempCarMask,0,0//draw the mask in		        		        carCanvas.Backdrop = p		      end if		    else		      MsgBox t_badpicture		    end if		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub SetClubLogo()		  Dim dlg as OpenDialog		  dim f as FolderItem		  dim w as FolderItem		  		  w = GetFolderItem("")		  		  dlg= New OpenDialog		  dlg.Title=t_selectanimagefile		  dlg.InitialDirectory = w.Child("theme")		  f=dlg.ShowModal()		  		  		  if f<>nil then		    G_Preferences.clubLogoPath=f.DisplayName		    dim p as Picture = Picture.Open(f)		    G_Preferences.ClubLogo=p		    		    //Store a copy of the image to the club_logo folder		    //dim f2 as FolderItem = GetFolderItem("club_logo").Child(f.Name)		    //f2.SaveAsPicture(G_Preferences.clubLogo)		    //copy the file		    dim f2 as FolderItem = GetFolderItem("club_logo")		    f.CopyFileTo(f2)		    		    //scale the picture to fit the canvase		    Canvas_GC_ClubLogo.Backdrop=scalePicture(G_Preferences.ClubLogo, Canvas_GC_ClubLogo.Width, Canvas_GC_ClubLogo.Height, True)//just to proove it's reading correctly		    G_Preferences.write("clubLogo",f.Name)//so it updates the change		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub SetDefaults()		  dim i as Integer		  		  //General Config Screen		  CheckBox_GC_Audio.Value = G_Preferences.readB("useAudio")		  CheckBox_ST_DisableLiveGraphing.Value = G_Preferences.readB("disableLiveGraphing")		  CheckBox_GC_CheckUpdate.Value = G_Preferences.readB("checkForUpdates")		  CheckBox_GC_AnnouceRanks.Value = G_Preferences.readB("rankAnnoucements")		  if G_Preferences.readS("editRacersColor") = "*" then		    CheckBox_GC_RacerColors.Value = True		  else		    CheckBox_GC_RacerColors.Value = False		  end if		  		 