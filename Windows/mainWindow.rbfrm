ALUES ('"+racer_ids(i)+"','"+str(currentEventID)+"');"		      		      //Check to see if we have a valid entry		      if UBound(csv_vals) <> 13 then		        MsgBox "Error importing, check file for valid format"		        return		      end if		      		      if csv_vals(0) <> "none" then		        //Make sure we did not get any null characters, this will create problems		        for i as integer=0 to UBound(csv_vals)		          if csv_vals(i) = chrb(&h00) then		            csv_vals(i) = ""		          end if		          		        next		        		        //Check to make sure that we do not have a duplicate record		        sql = "SELECT * FROM racer_table WHERE " + "name == '" + csv_vals(0) + "'" + " AND uid == '" + csv_vals(1) + "'"		        rs = G_Database.SQLSelect(sql)		        if rs.RecordCount > 0 then		          //Duplicate name and uid, warn user		          MsgBox "Error, duplicate racer import attempted"		        else		          //We must check to see if there is an existing record for this UID, if so we need to disable the new one		          sql = "SELECT * FROM racer_table WHERE UID=='"+csv_vals(1)+"'"		          rs = G_Database.SQLSelect(sql)		          if rs.RecordCount > 0 then		            //we have existing UID so we need to disable this one		            enabled = "0"		          else		            enabled = "1"		          end if		          		          		          sql = "INSERT INTO racer_table(name,uid,scale,chassis,class,make,model,motor,image,imagemask,name_pronounce,crystal,email,enabled) VALUES ('"  + csv_vals(0) +_		          "', '" + csv_vals(1) + "', '" + csv_vals(2) + "','" + csv_vals(3) +"', '" + csv_vals(4) + "', '" + csv_vals(5) + "', '" + csv_vals(6) + "','" + csv_vals(7) +_		          "', '" + csv_vals(8) + "', '" + csv_vals(9) + "', '" + csv_vals(10) + "', '" + csv_vals(11)+ "', '"  + csv_vals(12) + "', '" + enabled + "');"		          G_Database.SQLExecute(sql)		          		          if G_Database.Error = True then		            MsgBox "Error, possible duplicate UID"		          else		            G_Database.Commit		          end if		        end if		        		      end if		      		    wend		    PopulateUIDComboBox		    PopulateRacerListBox()		  end if		  		  GM_FillRacersControlStructure		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_GC_RacerExport	#tag Event		Sub Action()		  dim rs as RecordSet		  dim name,uid,scale,chassis,theClass,make,model,motor,image,imagemask,name_pronounce,crystal,email,enabled as string		  dim query as string		  		  Dim dlg as SaveAsDialog		  dim f as FolderItem		  dim g as FolderItem		  dim w as FolderItem		  dim d as new date		  		  w = GetFolderItem("")		  		  g = GetFolderItem("", 0)		  dlg= New SaveAsDialog		  dlg.Title=t_saveyourracecarinfo		  if TargetLinux = true then		    dlg.InitialDirectory = SpecialFolder.UserHome		  else		    dlg.InitialDirectory = SpecialFolder.Desktop		  end if		  dlg.SuggestedFileName = G_Preferences.readS("clubName")+"_racecars"+"_"+replaceall(d.SQLDateTime,":","-")+".txt"		  f=dlg.ShowModal()		  		  dim filestream as TextOutputStream		  		  if f<>nil then		    filestream = TextOutputStream.Create(f)		    query = "SELECT * from racer_table;"		    		    rs = G_Database.SQLSelect(query)		    if rs<>nil then		      while not rs.EOF		        //Make sure to not export deleted racers		        if rs.Field("hidden").BooleanValue = false then		          name = rs.Field("name").StringValue		          uid = rs.Field("uid").StringValue		          scale = rs.Field("scale").StringValue		          chassis = rs.Field("chassis").StringValue		          theClass = rs.Field("class").StringValue		          make = rs.Field("make").StringValue		          model = rs.Field("model").StringValue		          motor = rs.Field("motor").StringValue		          image = rs.Field("image").StringValue		          imagemask = rs.Field("imagemask").StringValue		          name_pronounce = rs.Field("name_pronounce").StringValue		          crystal = rs.Field("crystal").StringValue		          email = rs.Field("email").StringValue		          enabled = rs.Field("enabled").StringValue		          		          filestream.Writeline name+","+uid+","+scale+","+chassis+","+theClass+","+make+","+model+","+motor+","+image+","+imagemask+","+name_pronounce+","+crystal+","+email+","+enabled		        end if		        rs.MoveNext		      wend		    else		      MsgBox "Failed to export, error in query."+EndOfLine+EndOfLine+G_Database.ErrorMessage+EndOfLine+EndOfLine+query		    end if		    		    filestream.Close		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_GC_RacerInventory	#tag Event		Sub Action()		  G_Preferences.theRaceObject.RacerInventoryReport(G_Preferences.clubLogoPath, G_Preferences.readS("clubName"))		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_GC_Maint	#tag Event		Sub Action()		  //Announce what we are doing		  dim tempWindow as new messageWindow		  tempWindow.message = t_optimizing		  tempWindow.timeLimit = 100		  tempWindow.Show		  		  //Clean the database		  G_Database.SQLExecute("VACUUM")		  G_Database.Commit		  		  //Close the message window		  tempWindow.timeOut		End Sub	#tag EndEvent	#tag Event		Sub Open()		  if len(me.Caption) > 18 then		    me.TextSize = textsize12		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events selectDevicePopUpMenu	#tag Event		Sub Change()		  if windowOpened = true then		    ' We only want the button to be enabled		    ' if there's a valid selection		    openButton.Enabled = me.ListIndex <> -1		    //sendButton.Enabled = me.ListIndex <> -1		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  ' Populate the popup menu with all of the		  ' serial ports the system has installed.		  dim i, count as Integer		  dim status as boolean		  		  status = false		  count = System.SerialPortCount		  		  for i = 0 to