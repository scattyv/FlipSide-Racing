&h21		Private Sub DeleteRacer()		  dim i as Integer		  Dim n as Integer		  dim doDelete as Boolean		  		  //First make sure we have a selected record		  if M_selectedRecordSet = Nil or M_selectedRecordSet.RecordCount < 1 then		    MsgBox t_noracerselectedtodelete		    return		  end if		  		  n=MsgBox(t_areyousuredelete + " " + M_selectedRecordSet.Field("name").StringValue + "?"+endofline+endofline+t_oncedeletednorecovery,36)		  If n=6 then		    //user pressed Yes		    doDelete=true		  elseif n=7 then		    //user pressed No		    doDelete=false		  end if		  		  if doDelete=true then		    for i=0 to UBound(G_Preferences.theRaceObject.racers)		      if G_Preferences.theRaceObject.racers(i).theName=M_selectedRecordSet.Field("name").StringValue and G_Preferences.theRaceObject.racers(i).UID=M_selectedRecordSet.Field("uid").StringValue then		        G_Preferences.theRaceObject.racers.Remove(i)//we found the racer we need to remove from the control structure~		        Exit		      end if		    next		    //Instead of completely deleting record we mark it disabled and hidden		    //This way existing races that had this racer in them will still show the racers name since the record still exists		    M_selectedRecordSet.Edit		    M_selectedRecordSet.Field("enabled").BooleanValue =False		    M_selectedRecordSet.Field("hidden").BooleanValue = True		    M_selectedRecordSet.Update		    		    G_Database.Commit()//save the changes~		    PopulateUIDComboBox		    PopulateRacerListBox()//to reflect the changes		    //select the first record as we have destroyed the current record		    RacerListBox.ListIndex = 0		  end if//otherwise assume a 'no'		  		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub DisableInRaceControls()		  		  startStopButton.Enabled=true		  startRaceButton.Enabled=false		  		  		  		  //raceTypePopUpBox.Enabled=false		  //lapsField.Enabled=false		  //addToLapsButton.Enabled=false		  //removeFromLapsButton.