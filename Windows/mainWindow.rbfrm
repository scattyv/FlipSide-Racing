 Y As Integer) As Boolean		  PagePanel1.Value=1		End Function	#tag EndEvent	#tag Event		Sub MouseEnter()		  if me.Enabled=true then		    me.Backdrop=raceConfig_MouseOver		  end if		End Sub	#tag EndEvent	#tag Event		Sub MouseExit()		  me.Backdrop=raceConfig_Button		End Sub	#tag EndEvent	#tag Event		Sub Paint(g As Graphics, areas() As REALbasic.Rect)		  if fsTheme.version > 1 then		    dim center as integer		    g.ForeColor = fsTheme.navigationText		    g.TextSize = 12		    g.Bold = True		    center = (120 - g.StringWidth(t_raceconfig))/2		    g.DrawString(t_raceconfig,center,20)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events editRacersCButton	#tag Event		Function MouseDown(X As Integer, Y As Integer) As Boolean		  PagePanel1.Value=3		End Function	#tag EndEvent	#tag Event		Sub MouseEnter()		  if me.Enabled=true then		    me.Backdrop=editRacers_MouseOver		  end if		End Sub	#tag EndEvent	#tag Event		Sub MouseExit()		  me.Backdrop=editRacers_Button		End Sub	#tag EndEvent	#tag Event		Sub Paint(g As Graphics, areas() As REALbasic.Rect)		  if fsTheme.version > 1 then		    dim center as integer		    g.ForeColor = fsTheme.navigationText		    g.TextSize = 12		    g.Bold = True		    center = (120 - g.StringWidth(t_editracers))/2		    g.DrawString(t_editracers,center,20)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events raceScreenCButton	#tag Event		Function MouseDown(X As Integer, Y As Integer) As Boolean		  PagePanel1.Value=4		  AnimateRaceScreen()		End Function	#tag EndEvent	#tag Event		Sub MouseEnter()		  if me.Enabled=true then		    me.Backdrop=raceScreen_MouseOver		  end if		End Sub	#tag EndEvent	#tag Event		Sub MouseExit()		  me.Backdrop=raceScreen_Button		End Sub	#tag EndEvent	#tag Event		Sub Paint(g As Graphics, areas() As REALbasic.Rect)		  if fsTheme.version > 1 then		    dim center as integer		    g.ForeColor = fsTheme.navigationText		    g.TextSize = 12		    g.Bold = True		    center = (120 - g.StringWidth(t_racescreen))/2		    g.DrawString(t_racescreen,center,20)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events statisticsCButton	#tag Event		Function MouseDown(X As Integer, Y As Integer) As Boolean		  PagePanel1.Value=5		  		  //If a race is not active we want to refresh the results just in case something changed		  if M_RaceActive = false then		    dim currentRacer as integer		    currentRacer = ListBox_ST_Racers.ListIndex		    if ListBox_ST_Events.ListIndex > -1 then		      ShowRacerData(WhatEventIsSelected())		      //Check to see if we had a racer selected if so check to see if that many rows still exist and select that row		      if currentRacer > -1 then		        //We had a racer selected, select them again		        if ListBox_ST_Racers.ListCount >= currentRacer then		          ListBox_ST_Racers.ListIndex = currentRacer		          		        end if		      end if		    end if		  end if		End Function	#tag EndEvent	#tag Event		Sub MouseEnter()		  if me.Enabled=true then		    me.Backdrop=statistics_MouseOver		  end if		End Sub	#tag EndEvent	#tag Event		Sub MouseExit()		  me.Backdrop=statistics_button		End Sub	#tag EndEvent	#tag Event		Sub Paint(g As Graphics, areas() As REALbasic.Rect)		  if fsTheme.version > 1 then		    dim center as integer		    g.ForeColor = fsTheme.navigationText		    g.TextSize = 12		    g.Bold = True		    center = (120 - g.StringWidth(t_statistics))/2		    g.DrawString(t_statistics,center,20)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events generalConfigCButton	#tag Event		Function MouseDown(X As Integer, Y As Integer) As Boolean		  PagePanel1.Value=6		End Function	#tag EndEvent	#tag Event		Sub MouseExit()		  me.Backdrop=generalConfig_Button		End Sub	#tag EndEvent	#tag Event		Sub MouseEnter()		  if me.Enabled=true then		    me.Backdrop=generalConfig_MouseOver		  end if		End Sub	#tag EndEvent	#tag Event		Sub Paint(g As Graphics, areas() As REALbasic.Rect)		  if fsTheme.version > 1 then		    dim center as integer		    g.ForeColor = fsTheme.navigationText		    g.TextSize = 12		    g.Bold = True		    center = (120 - g.StringWidth(t_generalconfig))/2		    g.DrawString(t_generalconfig,center,20)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events raceTimer	#tag Event		Sub Action()		  //The job of this timer is to do the best it can to sync to the system clock timer		  		  dim newTime as Double = (Microseconds - M_StartTime)/1000000//should then put this in seconds and correctly be displaying the time as it goes down.		  dim temp as string		  dim timeLeft as Double		  dim timeAccumulated as Double		  dim FinishCount as Integer		  dim activeIndexes(-1) as integer = FindActiveIndexes()//gets the indexes for the M_Slots that are active		  static frames as integer = 0 //we use this to limit how often we redraw the timer		  static maxframes as integer = 3 //This limits how many times this timer executes before we update the display		  		  frames = frames + 1 //Increment the frame counter so we know if it is time to display or not		  		  //Switched to polling method, this is no longer needed		  //if G_Preferences.hardwareType = "Kyosho IC" then		  //if trackUIDtimer.Mode=0 then		  //trackUIDtimer.Mode=2		  //end if		  //end if		  		  temp = format(newTime,"0.00")		  newTime = cdbl(temp)//grabs lap time regardless, '0' is already stored for the first time position~		  		  timeAccumulated = G_Preferences.theRaceObject.tempTimeElapsed + newTime		  G_Preferences.theRaceObject.timeElapsed = timeAccumulated		  		  timeLeft = G_Preferences.theRaceObject.tempTimeLimit - newTime		  G_Preferences.theRaceObject.timeRemaining = timeLeft		  		  //Do different functions for each race type		  Select Case G_Preferences.theRaceObject.RaceType		    		  Case "Timed"		    		    if PagePanel1.Value=4 then//so it doesn't flicker on other window (WINDOWS FIX)		      if G_Preferences.theRaceObject.timeRemaining>=0 then		        //Format text in DD:HH:MM:SS.SS format		        //timeRemainingText=Format(G_Preferences.theRaceObject.timeRemaining,"0.00")		        timeRemainingText=secondsToTime(G_Preferences.theRaceObject.timeRemaining, "#.##")		        		        		      end if		    end if		    		    		    //Announce time remaining and rank at specific intervals		    if G_Preferences.theRaceObject.timeRemaining>=0 then		      speakTimeRemaining(G_Preferences.theRaceObject.timeRemaining)		      speakRank(G_Preferences.theRaceObject.timeRemaining)		      		      //Check to see if we need to start the ending count down timer		      //There is a delay to text to speech so we add a bit of a buffer		      if G_Preferences.theRaceObject.timeRemaining-1.1<=G_Preferences.theRaceObject.endingCountDown and G_Preferences.theRaceObject.endingCountDown <> 0 then		        //we need to make sure the ending count down timer has started by this point		        if endCountDownTimer.Mode = 0 then		          M_endCountDownTime = G_Preferences.theRaceObject.endingCountDown		          endCountDownTimer.Mode = 2		        end		        		      end if		      		    end if		    		    		    		    		    if G_Preferences.theRaceObject.timeRemaining<=0 then//the race has ended for the timed lap		      mainWindow.timeRemainingText = "0.00"		      if M_RaceEnding=false then		        M_RaceEnding=true		        		        //Turn on relayboard light if enabled		        if G_Preferences.relayPort <> "None" then		          relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		          relayBoard.enablePort(G_Preferences.relayPort, 2, "a")		          relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		          relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		        end if		        		        SerialLapCounters1.lightState("ending")		        		        //Play end the race sound effect so racers know race as ended and they should complete their final lap		        if G_Preferences.useAudio=true then		          //Check to see if ending horn is enabled		          if G_Preferences.theRaceObject.endinghorn = True then		            //Play sound and delay text		            fsTheme.endingHorn.Play		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(G_Preferences.speechText.Value("timelimitreached"), false)		          else		            robotSpeak(G_Preferences.speechText.Value("timelimitreached"), true)		          end if		        end if		        for i as Integer = 0 to UBound(activeIndexes)		          M_Slots(activeIndexes(i)).EndingRace=true		        next		      else//do a different kind of check in here to see if all the racers have the RaceEnded Flag to True or Not~		        for i as Integer = 0 to UBound(activeIndexes)		          //If the person has no laps go ahead and end the race for them		          if M_Slots(activeIndexes(i)).lapCount < 1 then		            M_Slots(activeIndexes(i)).raceEnded = true		          end if		          		          if M_Slots(activeIndexes(i)).raceEnded=true then		            FinishCount=FinishCount+1		          end if		        next		        if FinishCount=UBound(activeIndexes)+1 then		          EndTheRace()//then it's time to the end the race.		        end if		      end if		      		      //End the race if timeout is exceeded		      if G_Preferences.theRaceObject.timeOut + G_Preferences.theRaceObject.timeRemaining <= 0 then		        EndTheRace()		      end if		      		    end if		    		    		  Case "Staggered Time"		    //In this race type ever racer has their own clock that determines when they are done		    //You must use the laptimeDeviation to determine if they have exceeded their time limit.		    //Each racer then also has their own timeout to watch for		    		    if PagePanel1.Value=4 then//so it doesn't flicker on other window (WINDOWS FIX)		      if G_Preferences.theRaceObject.timeRemaining>=0 then		        //Format text in DD:HH:MM:SS.SS format		        //timeRemainingText=Format(G_Preferences.theRaceObject.timeRemaining,"0.00")		        timeRemainingText=secondsToTime(G_Preferences.theRaceObject.timeRemaining, "#.##")		      end if		      		      //Refresh each racers personal timer on the Window if enough frames have passed		      if frames > maxframes then		        for i as Integer = 0 to UBound(activeIndexes)		          for j as Integer = 0 to 49		            if M_Slots(activeIndexes(i)).theName = NameRankText(j).Text then		              //We have a match so update that time to reflect it		              //But first check to make sure they have crossed the finish line for the first time		              if M_Slots(activeIndexes(i)).firstPass > 0 then		                LapTimeText(j).TextColor = RankText(j).textColor		                if G_Preferences.theRaceObject.timeRemaining + M_Slots(activeIndexes(i)).laptimeDeviation >= 0 then		                  LapTimeText(j).Text = secondsToTime(G_Preferences.theRaceObject.timeRemaining + M_Slots(activeIndexes(i)).laptimeDeviation, "#.##")		                elseif M_Slots(activeIndexes(i)).EndingRace = True and M_Slots(activeIndexes(i)).raceEnded = False then		                  LapTimeText(j).Text = "0.00"		                  		                  //If less than 0 Seconds we show the final time using the standard animate from index method		                  //else		                  //LapTimeText(j).Text = "0.00"		                  		                end if		                //Check if enough frames have passed that it is time to redraw		                		                ////DB_RS_TIME(j).Refresh		                		              end if		            end if		          next		        next		      end if		      		    end if		    		    //No need to check individual timeouts if the overall time limit has not been reached		    if G_Preferences.theRaceObject.timeRemaining<=0 then//the race has ended for the timed lap		      mainWindow.timeRemainingText = "0.00"		      //Set the relayboard to light 2 to signify race is ending soon.		      if M_RaceEnding=false then		        M_RaceEnding=true		        //Turn on relayboard light if enabled		        if G_Preferences.relayPort <> "None" then		          relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		          relayBoard.enablePort(G_Preferences.relayPort, 2, "a")		          relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		          relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		        end if		        SerialLapCounters1.lightState("ending")		      end if		      		      		      //Go through each racer to determine if they have exceeded their time limit		      //Also check to see if they have exceeded their timeout		      for i as Integer = 0 to UBound(activeIndexes)		        		        //End race for racers that have not completed a lap		        if M_Slots(activeIndexes(i)).lapCount < 1 then		          M_Slots(activeIndexes(i)).raceEnded = true		        end if		        		        //Check to see if racer is still active		        if M_Slots(activeIndexes(i)).raceEnded = False then		          		          //Check to see if they have exceeded their individual time limit		          if M_Slots(activeIndexes(i)).EndingRace = False then		            if M_Slots(activeIndexes(i)).laptimeDeviation + G_Preferences.theRaceObject.timeRemaining <= 0 then		              M_Slots(activeIndexes(i)).EndingRace = True		              		              //Announce for each racer that they have met the time limit		              if G_Preferences.useAudio=true then		                robotSpeak(M_Slots(activeIndexes(i)).speakName + ", " + G_Preferences.speechText.Value("timelimitreached"), true)		              end if		              		            end if		            		          end if		          		          //Check to if timeout is exceeded		          if M_Slots(activeIndexes(i)).laptimeDeviation +G_Preferences.theRaceObject.timeOut + G_Preferences.theRaceObject.timeRemaining <= 0 then		            //They should be timed out, announce it and update the screen.		            M_Slots(activeIndexes(i)).raceEnded = True		            robotSpeak(M_Slots(activeIndexes(i)).speakName + ", " + G_Preferences.speechText.Value("timedout"), false)		            AnimateRaceScreen		          end if		          		        end if		        		        //Check to see how many racers are done		        if M_Slots(activeIndexes(i)).raceEnded=true then		          FinishCount=FinishCount+1		        end if		        		      next		      		      if FinishCount=UBound(activeIndexes)+1 then		        //Recalculate places just in case		        //Set race type to timed to recalculate so it does not estimate based on current clock		        G_Preferences.theRaceObject.RaceType = "Timed"		        G_Preferences.theRaceObject.CalculatePlace(-1,0)		        G_Preferences.theRaceObject.RaceType = "Staggered Time"		        EndTheRace()//then it's time to the end the race.		      end if		      		    end if		    		  Case "Lap"		    timeAccumulated = G_Preferences.theRaceObject.tempTimeElapsed + newTime		    G_Preferences.theRaceObject.timeElapsed = timeAccumulated		    if PagePanel1.Value=4 then		      //Format text in the DD:HH:MM:SS.SS format		      //timeRemainingText=Format(G_Preferences.theRaceObject.timeElapsed,"0.00")		      timeRemainingText=secondsToTime(G_Preferences.theRaceObject.timeElapsed, "#.##")		    end if		    		    		    //make sure we have racers to check		    if UBound(activeIndexes) > -1 then		      //Say the ranking at specified interval		      speakRank(G_Preferences.theRaceObject.timeElapsed)		      		      if M_RaceEnding=false then		        for i as integer = 0 to UBound(activeIndexes)		          if M_Slots(activeIndexes(i)).lapCount>=G_Preferences.theRaceObject.numLaps then		            //FinishCount=FinishCount+1		            if M_Slots(activeIndexes(i)).raceEnded=false then		              M_Slots(activeIndexes(i)).raceEnded=true//end the race for this racer		              		              //Set ended time for use with timeouts		              G_Preferences.theRaceObject.endedTime = G_Preferences.theRaceObject.timeElapsed		              		              //Play end of laps sound for racer		              if G_Preferences.useAudio=true then		                //Check to see if the ending horn is enabled		                if G_Preferences.theRaceObject.endinghorn = true then		                  //Play ending horn and add a .5 second delay before speaking		                  fsTheme.endingHorn.Play		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(M_Slots(activeIndexes(i)).speakName + " " + G_Preferences.speechText.Value("finished") + ".  ", false)		                else		                  robotSpeak(M_Slots(activeIndexes(i)).speakName + " " + G_Preferences.speechText.Value("finished") + ".  ", true)		                end if		                robotSpeak(" " + G_Preferences.speechText.Value("laplimitreached"),false)		                //animate race screen to reflect racer finished		                AnimateRaceScreen		              end if		            end if		            		            //When the first person reaches the lap limit everyone else has to complete there final lap.		            M_RaceEnding=true		            for j as Integer = 0 to UBound(activeIndexes)		              M_Slots(activeIndexes(j)).EndingRace=true		            next		            		            //Turn on relayboard light if enabled		            if G_Preferences.relayPort <> "None" then		              relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		              relayBoard.enablePort(G_Preferences.relayPort, 2, "a")		              relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		              relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		            end if		            SerialLapCounters1.lightState("ending")		          end if		        next		      end if		      		      if M_RaceEnding=true then		        for i as Integer = 0 to UBound(activeIndexes)		          if M_Slots(activeIndexes(i)).raceEnded=true then		            FinishCount = FinishCount + 1		          end if		          if FinishCount=UBound(activeIndexes)+1 then		            		            EndTheRace()		          end if		        next		        		        //Check the timeout, only triggers after one person has completed the laps necessary		        if G_Preferences.theRaceObject.timeElapsed - G_Preferences.theRaceObject.endedTime > G_Preferences.theRaceObject.timeOut then		          EndTheRace()		        end if		        		      end if		      		    end if		    		  Case "Lap and Timed"		    //Timed limits for the race		    		    if PagePanel1.Value=4 then//so it doesn't flicker on other window (WINDOWS FIX)		      if G_Preferences.theRaceObject.timeRemaining>=0 then		        //Format time in the DD:HH:MM:SS.SS format		        //timeRemainingText=Format(G_Preferences.theRaceObject.timeRemaining,"0.00")		        timeRemainingText=secondsToTime(G_Preferences.theRaceObject.timeRemaining, "#.##")		      end if		    end if		    		    //Announce time remaining and rank at specific intervals		    if G_Preferences.theRaceObject.timeRemaining>=0 then		      speakTimeRemaining(G_Preferences.theRaceObject.timeRemaining)		      speakRank(G_Preferences.theRaceObject.timeRemaining)		      		      //Check to see if we need to start the ending count down timer		      //There is a delay to text to speech so we add a bit of a buffer		      if G_Preferences.theRaceObject.timeRemaining-1.1<=G_Preferences.theRaceObject.endingCountDown and G_Preferences.theRaceObject.endingCountDown <> 0 then		        //we need to make sure the ending count down timer has started by this point		        if endCountDownTimer.Mode = 0 then		          M_endCountDownTime = G_Preferences.theRaceObject.endingCountDown		          endCountDownTimer.Mode = 2		        end		        		      end if		      		    end if		    		    		    		    		    if G_Preferences.theRaceObject.timeRemaining<=0 then//the race has ended for the timed lap		      mainWindow.timeRemainingText = "0.00"		      if M_RaceEnding=false then		        M_RaceEnding=true		        		        //Turn on relayboard light if enabled		        if G_Preferences.relayPort <> "None" then		          relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		          relayBoard.enablePort(G_Preferences.relayPort, 2, "a")		          relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		          relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		        end if		        SerialLapCounters1.lightState("ending")		        		        //Play end the race sound effect so racers know race as ended and they should complete their final lap		        if G_Preferences.useAudio=true then		          //Check to see if ending horn is enabled		          if G_Preferences.theRaceObject.endinghorn = True then		            //Play sound and delay text		            fsTheme.endingHorn.Play		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(" ", false)		            robotSpeak(G_Preferences.speechText.Value("timelimitreached"), false)		          else		            robotSpeak(G_Preferences.speechText.Value("timelimitreached"), true)		          end if		        end if		        for i as Integer = 0 to UBound(activeIndexes)		          M_Slots(activeIndexes(i)).EndingRace=true		        next		      else//do a different kind of check in here to see if all the racers have the RaceEnded Flag to True or Not~		        for i as Integer = 0 to UBound(activeIndexes)		          //If the person has no laps go ahead and end the race for them		          if M_Slots(activeIndexes(i)).lapCount < 1 then		            M_Slots(activeIndexes(i)).raceEnded = true		          end if		          		        next		        		      end if		      		    end if		    		    //Lap limits for race		    //make sure we have racers to check		    if UBound(activeIndexes) > -1 then		      		      if M_RaceEnding=false then		        for i as integer = 0 to UBound(activeIndexes)		          if M_Slots(activeIndexes(i)).lapCount>=G_Preferences.theRaceObject.numLaps then		            //FinishCount=FinishCount+1		            if M_Slots(activeIndexes(i)).raceEnded=false then		              M_Slots(activeIndexes(i)).raceEnded=true//end the race for this racer		              		              //Set ended time for use with timeouts		              G_Preferences.theRaceObject.endedTime = G_Preferences.theRaceObject.timeRemaining		              		              //Play end of laps sound for racer		              if G_Preferences.useAudio=true then		                //Check to see if the ending horn is enabled		                if G_Preferences.theRaceObject.endinghorn = true then		                  //Play ending horn and add a .5 second delay before speaking		                  fsTheme.endingHorn.Play		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(" ", false)		                  robotSpeak(M_Slots(activeIndexes(i)).speakName + " " + G_Preferences.speechText.Value("finished") + ".  ", false)		                else		                  robotSpeak(M_Slots(activeIndexes(i)).speakName + " " + G_Preferences.speechText.Value("finished") + ".  ", true)		                end if		                //animate race screen to reflect racer finished		                AnimateRaceScreen		              end if		            end if		            		            //When the first person reaches the lap limit everyone else has to complete there final lap.		            M_RaceEnding=true		            for j as Integer = 0 to UBound(activeIndexes)		              M_Slots(activeIndexes(j)).EndingRace=true		            next		            		            //Turn on relayboard light if enabled		            if G_Preferences.relayPort <> "None" then		              relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		              relayBoard.enablePort(G_Preferences.relayPort, 2, "a")		              relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		              relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		            end if		            SerialLapCounters1.lightState("ending")		            		          end if		        next		      end if		      		      if M_RaceEnding=true then		        for i as Integer = 0 to UBound(activeIndexes)		          if M_Slots(activeIndexes(i)).raceEnded=true then		            FinishCount = FinishCount + 1		          end if		          if FinishCount=UBound(activeIndexes)+1 then		            		            EndTheRace()		          end if		          //Check the timeout for a lap timeout		          if G_Preferences.theRaceObject.endedTime - G_Preferences.theRaceObject.timeRemaining > G_Preferences.theRaceObject.timeOut then		            EndTheRace()		          end if		          		          //End the race if timeout is exceeded after time runs out		          if G_Preferences.theRaceObject.timeOut + G_Preferences.theRaceObject.timeRemaining <= 0 then		            EndTheRace()		          end if		          		        next		        		      end if		      		    end if		    		  Case "Free Play"		    //This mode never ends, the timer just counts up		    //Racers are removed from the race after they time out so that other racers can join		    //Like staggered start each racer gets their own starting time, but they do not get a separate timer		    		    timeAccumulated = G_Preferences.theRaceObject.tempTimeElapsed + newTime		    G_Preferences.theRaceObject.timeElapsed = timeAccumulated		    if PagePanel1.Value=4 then		      //Format text in the DD:HH:MM:SS.SS format		      //timeRemainingText=Format(G_Preferences.theRaceObject.timeElapsed,"0.00")		      timeRemainingText=secondsToTime(G_Preferences.theRaceObject.timeElapsed, "#.##")		    end if		    		    //make sure we have racers to check		    if UBound(activeIndexes) > -1 then		      //Need to check timeout for each racer and remove them if exceeded		      for i as integer = 0 to UBound(activeIndexes)		        		        if ubound(M_Slots(activeIndexes(i)).laptimes) >= 0 then		          if G_Preferences.theRaceObject.timeElapsed > M_Slots(activeIndexes(i)).lapTimes(UBound(M_Slots(activeIndexes(i)).laptimes)) + G_Preferences.theRaceObject.timeOut + M_Slots(activeIndexes(i)).laptimeDeviation then		            //Racer has exceeded the timeout and needs removed		            RemoveRacerSlot(activeIndexes(i))		            		          end if		        elseif G_Preferences.theRaceObject.timeElapsed > M_Slots(activeIndexes(i)).laptimeDeviation + G_Preferences.theRaceObject.timeOut then		          //Racer has exceeded the timeout and needs removed		          RemoveRacerSlot(activeIndexes(i))		        end if		      next		      		    end if		    		  Case "Drag"		    //This mode counts up until it hits the timeout.  We have no way of knowing how many people are in the drag race so we can not end automatically.		    		    timeAccumulated = G_Preferences.theRaceObject.tempTimeElapsed + newTime		    G_Preferences.theRaceObject.timeElapsed = timeAccumulated		    if PagePanel1.Value=4 then		      timeRemainingText=secondsToTime(G_Preferences.theRaceObject.timeElapsed, "#.##")		    end if		    		    if timeAccumulated >= G_Preferences.theRaceObject.timeOut then		      EndTheRace		    end if		    		    		  else//for safety reasons		    MsgBox "Race Timer: -> Error unknown race type!"		  End Select		  		  //Refresh the time remaining double buffer graphic		  		  if frames > maxframes then		    frames = 0		    DB_TimeRemaining.Refresh		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events countDownTimer	#tag Event		Sub Action()		  G_Preferences.theRaceObject.state = "started"		  TimeHeaderText.Text=t_countdown + ": "		  		  if G_Preferences.theRaceObject.countdowntype = "F1" then		    timeRemainingText = "0"		  else		    timeRemainingText=cstr(M_CountDownTime)		  end if		  		  if G_Preferences.theRaceObject.state<>"paused" then		    if G_Preferences.hardwareType = "keyboard" or SerialLapCounters1.canPause = True then		      //Pausing supported		      startStopButton.Caption=t_pause		    else		      //We need to disable the button for these hardware types since they do not support pausing		      startStopButton.Enabled = false		    end if		  end if		  		  select case M_CountDownTime		  case 0		    countDownCanvas.Backdrop=Countdown_0		    if G_Preferences.useAudio=true then		      if G_Preferences.S_Countdown0 <> nil then		        G_Preferences.S_Countdown0.Play		      end if		    end if		  case 1		    countDownCanvas.Backdrop=Countdown_1		    		    //Check to see if we are in F1 style countdown, if so play the beep sound and randomize how long it will take for the race to start		    if G_Preferences.theRaceObject.countdowntype = "F1" then		      fsTheme.countdownBeep.Play		      dim r as New Random		      		      me.Period = r.InRange(1000,10000)		      		    else		      robotspeak(G_Preferences.speechText.Value("countdownone"),true)		    end if		    		    //Turn on relayboard light if enabled		    if G_Preferences.relayPort <> "None" then		      relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		      relayBoard.enablePort(G_Preferences.relayPort, 2, "a")		      relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		      relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		    end if		    SerialLapCounters1.lightState("two")		    		  case 2		    countDownCanvas.Backdrop=Countdown_2		    		    if G_Preferences.theRaceObject.countdowntype = "F1" then		      fsTheme.countdownBeep.Play		    else		      robotspeak(G_Preferences.speechText.Value("countdowntwo"),true)		    end if		    		    //Turn on relayboard light if enabled		    if G_Preferences.relayPort <> "None" then		      relayBoard.enablePort(G_Preferences.relayPort, 1, "b")		      relayBoard.enablePort(G_Preferences.relayPort, 2, "b")		      relayBoard.enablePort(G_Preferences.relayPort, 3, "a")		      relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		    end if		    SerialLapCounters1.lightState("three")		    		  case 3		    countDownCanvas.Backdrop=Countdown_3		    		    if G_Preferences.theRaceObject.countdowntype = "F1" then		      fsTheme.countdownBeep.Play		    else		      robotspeak(G_Preferences.speechText.Value("countdownthree"),true)		    end if		    		  case 4		    countDownCanvas.Backdrop=Countdown_4		    		    if G_Preferences.theRaceObject.countdowntype = "F1" then		      fsTheme.countdownBeep.Play		    else		      robotspeak(G_Preferences.speechText.Value("countdownfour"),true)		    end if		    		  case 5		    countDownCanvas.Backdrop=Countdown_5		    		    if G_Preferences.theRaceObject.countdowntype = "F1" then		      fsTheme.countdownBeep.Play		    else		      robotspeak(G_Preferences.speechText.Value("countdownfive"),true)		    end if		    		    //This is now text to speech and triggered before the timer		    //case 10		    //countDownCanvas.Backdrop=Countdown_10		    //if G_Preferences.useAudio=true then		    //if G_Preferences.S_PrepareToRace <> nil then		    //G_Preferences.S_prepareToRace.Play		    //end if		    //end if		  else		    //In F1 style countdown beep each second that passes		    if G_Preferences.theRaceObject.countdowntype = "F1" then		      fsTheme.countdownBeep.Play		    end if		    		  end Select		  if M_CountDownTime <= 0 then		    //Start the hardware counter for hardware that supports it		    SerialLapCounters1.resetTimer		    SerialLapCounters2.resetTimer		    //Wait for SerialLapCounter1 to finish transmitting in order to be as close to in sync as possible		    SerialLapCounters1.XmitWait		    		    //reset the interval just in case		    me.Period = 1000		    countDownTimerActive=false		    me.Mode=0		    dim initialTimeCheck as double = Microseconds		    dim i as Integer		    for i=0 to 49		      if M_Slots(i)<>nil then		        if M_Slots(i).Active=true then		          M_Slots(i).timeSinceLastCheck= initialTimeCheck//so it calculates time correctly from start~		        end if		      end if		    next		    		    //Turn on relayboard light if enabled		    if G_Preferences.relayPort <> "None" then		      relayBoard.enablePort(G_Preferences.relayPort, 1, "a")		      relayBoard.enablePort(G_Preferences.relayPort, 2, "b")		      relayBoard.enablePort(G_Preferences.relayPort, 3, "b")		      relayBoard.enablePort(G_Preferences.relayPort, 4, "b")		    end if		    SerialLapCounters1.lightState("start")		    		    G_Preferences.theRaceObject.tempTimeLimit = G_Preferences.theRaceObject.timeLimit//get the value of the time limit and put into the temporary time limit for pausing purposes		    		    M_StartTime = Microseconds//so we have time to reference from~		    M_RaceActive = true		    raceTimer.Mode=2//the race timer will handle when a 1/100th of a second has gone by.		    		    //If this is the Core lap counter we need to constantly send inventory requests to be able to see RFID's		    if G_Preferences.hardwareType = "CoreSpeedway" then		      if trackUIDtimer.Mode=0 then		        trackUIDtimer.Mode=2		      end if		    elseif G_Preferences.hardwareType = "CoreSpeedway i166" then		      Thread_Core166.Kill		      Thread_Core166.Run		    end if		    		    Select Case G_Preferences.theRaceObject.RaceType		    Case "Timed"		      TimeHeaderText.text=t_timeremaining + ": "		    Case "Lap"		      TimeHeaderText.Text=t_timeelapsed + ": "		    Case "Lap and Timed"		      TimeHeaderText.Text=t_timeremaining + ": "		    Case "Staggered Time"		      TimeHeaderText.text=t_timeremaining + ": "		    Case "Free Play"		      TimeHeaderText.Text=t_timeelapsed + ": "		    Case "Drag"		      TimeHeaderText.Text=t_timeelapsed + ": "		    else//for safety reasons		      MsgBox "CalculatePace: -> Error unknown race type!"		    End Select		    		    		  end if		  M_CountDownTime = M_CountDownTime - 1		  //Refresh the time remaining graphic		  DB_TimeRemaining.Refresh		End Sub	#tag EndEvent#tag EndEvents#tag Events Shell_CustomHardware	#tag Event		Sub DataAvailable()		  M_shellBuffer = M_shellBuffer + Me.ReadAll		End Sub	#tag EndEvent#tag EndEvents#tag Events Serial1	#tag Event		Sub DataAvailable()		  dim rawbytes as string = me.ReadAll		  //Changed to property of Window so it can be easily cleared		  //static assembledHex() as string		  dim hexCode as string		  dim HexCommandCount as Integer		  dim flagStart,flagEnd,dataStart as Integer = -1		  dim packetLength as Integer = -1		  dim temp as string		  dim uid,tempUID as string		  dim uids() as string		  dim byteCount as Integer		  dim rs as RecordSet		  		  		  dim reader as new tiS6350		  		  hexCode = convertToHex(rawbytes)		  //invResField.Text = hexCode//print to screen so we can see it working~		  		  //Fill the array with the hex commands as they come in~		  HexCommandCount = countfieldsb(hexCode," ")		  for i as Integer=1 to HexCommandCount		    assembledHex.Append nthfieldb(hexCode," ",i)		  next		  		  //errorText.Text="Attempting Assemble..."		  		  'Byte 1 is the Start of Frame, always 01		  'Byte 2 and 3 is the length of the command including the 2 byte check sum that is added to the end, Least Significant Byte first		  'Byte 4 and 5 is the node address, leave at 00 as we do not reference nodes		  'Byte 6 is the command flags, no flags present		  'Byte 7 is the command, &h60 means it is an ISO15693 command which will be in the data portion of the packet		  'Bytes 8&9 are ISO settings.		  'Bytes '10+n' is the data with the UIDs		  'Last 2 Bytes are the checksum		  		  //Sample Response with 4 UIDs.		  //01 35 00 00 00 00 60 41 82 00 00 00 00 80 14 C0 12 00 00 07 E0 00 00 B6 E7 53 13 00 00 07 E0 00 00 79 14 C0 12 00 00 07 E0 00 00 7F 14 C0 12 00 00 07 E0 C6 39		  		  //Data may come in however at random intervals, therefor, a character by character analysis method has been put into place below		  		  for i as Integer=0 to UBound(assembledHex)		    //infoText.Text="i="+str(i)+"    UBound(assembledHex)="+str(UBound(assembledHex))		    if assembledHex(i)="01" then		      if ubound(assembledHex)>=(i+6) then//must be enough data so far so that we can even do the check at i+6		        if assembledHex(i+6)="60" then		          flagStart=i//valid command found, a 01 followed by 6 bytes and a 60 ISO command doesn't happen by chance.		          dataStart=i+11//11 ahead is where the data starts~		          //errorText.Text="Data Points Found"		        ElseIf assembledhex(i+6)="F0" then		          //We have a version packet and need to act accordingly		          if UBound(assembledhex)>=(i+11) then		            //we have enough data to get the version number		            if assembledHex(i+9) = "09" then		              //We have the infinity 166 module so we need to slow down		              hardwareversion = "09"		              trackUIDtimer.Period = 31		            ElseIf assembledHex(i+9) = "07" then		              trackUIDtimer.Period = 10		              hardwareversion = "07"		            end if		            		            //Erase this section now		            for m as Integer=1 to i + 11		              assembledHex.Remove(0)//keeps removing until it's gotten to the packetLength, leaving just anything else that may be there for analysis next round or perhaps this round still.		            next		            		          end if		          		          		        end if		      else		        Return//no point continuing on, we obviously don't have the rest of what we need yet to bother checking packetLengths yet.		      end if		    end if		    		    if flagStart<>-1 and packetLength=-1 then//if we have a valid command coming in, and packetLength has not been decided lets decide it.		      packetLength = val("&h" +assembledHex(flagStart+1))+val("&h"+assembledHex(flagStart+2))//bytes 2 and 3 from the start describe the length~, so start+1, start+2		      //errorText.Text="Packet Length Found"		    end if		    		    if flagStart<>-1 then		      temp=temp+assembledHex(i)+" "		      //revisedResField.Text=temp		      //infoText.Text="i="+str(i)+"   flagstart="+str(flagStart)+"    packetLength="+str(packetLength)		    end if		    //MsgBox "i="+str(i)+endOfLine+endOfLine+" flagStart="+str(flagStart)+endofLine+"abs(i-flagStart)+1="+str(abs(i-flagStart)+1)+endOfLine+"Packet Length="+str(packetLength)		    if abs(i-flagstart)+1=packetLength then		      flagEnd=i		      tempUID=""		      //MsgBox "Packet Length Verified (Packet Below)"+EndOfLine+EndOfLine+temp		      		      //Do stuff to get UID here, -2 on flagEnd because we ignore checksum.		      for k as Integer=(flagEnd-2) downto dataStart		        byteCount=byteCount+1		        tempUID=tempUID+assembledHex(k)		        if byteCount=8 then		          //MsgBox "UID="+tempUID		          uids.Append tempUID		          byteCount=0		          tempUID=""		          if k-1>dataStart then		            k=k-2//skip 2 positions now, the third will be where we start again correctly when loop comes around~		          end if		        end if		      next		      		      //Erase this section now		      for  k as Integer=1 to flagstart + packetLength		        assembledHex.Remove(0)//keeps removing until it's gotten to the packetLength, leaving just anything else that may be there for analysis next round or perhaps this round still.		      next		      		      temp=""		      for k as Integer=0 to UBound(assembledHex)		        temp=temp+assembledHex(k)		      next		      //MsgBox "assembledHex Test="+EndOfLine+EndOfLine+temp		      		      //Clear Variables		      flagStart=-1		      flagEnd=-1		      packetLength=-1		      dataStart=-1		      temp=""		      tempUID=""		      //errorText.Text="Assemble Completed"		    end if		  next		  		  //errorText.Text="Added UIDs"		  		  if UBound(uids)>=0 then		    //Moved below only really needed if we are selecting or editing racers		    //rs = G_database.SQLSelect("SELECT * FROM racer_table WHERE " + "uid == '" + uids(0) + "'")		    		    //if rs.Field("uid").StringValue=uids(0) then		    		    //Need to add catch here to only do a check if one of the UID's is valid for being checked~		    //If at least one is valid then allow the checks below~		    		    if M_RaceActive=true then//should only register UID's when the race is active~ //ACTIVE RACE		      		      //Fix me		      //JDA		      //Sloppy way of silencing tags to speed up reads		      //make sure we do not have the infinity 166 module, if we do then do not do a silence		      //the infinity 166 module locks up if we communicate with it too frequently		      		      if lenb(rawbytes) > 22 then		        if G_Preferences.hardwareType = "CoreSpeedway i166" then		          tagToSilence = midb(rawbytes,14,8)		        else		          reader.silenceTag(midb(rawbytes,14,8),serial1)		        end if		      end if		      		      if M_GamePaused=false then//should also only register if the game isn't paused!		        for i as integer=0 to UBound(uids)		          uid = uids(i)		          G_Preferences.theRaceObject.CalculateNewRacerValues(uid,0)		          //No longer needed moved into the above method		          //G_Preferences.theRaceObject.CalculatePlace()//finds who is in first place and sets places accordingly for all racers		          		          //In calculateNewRaceValues it will animate for statistics screen & Race Screen only if minimum lap time has passed for at least one racer.		          //This is to prevent memory leak error inherit in the graphing code~		          		        next		      end if		      		    else//SELECTING OR EDIT RACERS~		      		      rs = G_database.SQLSelect("SELECT * FROM racer_table WHERE " + "uid == '" + uids(0) + "' AND enabled == 1")		      		      if M_SelectingRacers=true then//SELECTING RACERS		        		        //Make sure that the recieved UID matches an inactive racer before proceeding		        for k as integer=0 to UBound(uids)		          for l as integer=0 to UBound(G_Preferences.theRaceObject.racers)		            if G_Preferences.theRaceObject.racers(l).UID = uids(k) and G_Preferences.theRaceObject.racers(l).Active = false and G_Preferences.theRaceObject.racers(l).Enabled = True then		              //We have  match for an inactive racer so we can proceed		              G_Preferences.theRaceObject.AddRacerToRace(uids(k),l,0)		            end if		          next		        next		        		      else//EDIT RACERS		        if uidField.Text = "none" then		          //just set the UID text field as they want a new racer created		          uidField.Text = uids(0)		        elseif rs.Field("uid").StringValue = uids(0) then		          if scanUIDtimerShutDown=false then//so it only does it once		            scanUIDtimerShutDown=true		          end if		          uidField.Text=""		          // Populate edit racer feilds with results from previous query using method		          M_selectedRecordSet = rs		          carCanvas.Backdrop = Nil		          PopulateEditRacerInfo()//to reflect the change it will fill the editracer info with the item that is selected~		          		          for i as integer=0 to RacerListBox.ListCount-1		            if RacerListBox.cell(i,7)=M_selectedRecordSet.Field("uid").StringValue then		              RacerListBox.Selected(i)=true		            else		              RacerListBox.Selected(i)=false//deselect old selections		            end if		          next		        end if		      end if		      		    end if		    //else//must be a new racer, clear the fields for him? (might not want to do this, you decide)		    		    //end if		    		    uidField.Text=uids(0)		  end if		  		  		  		  		  		End Sub	#tag EndEvent#tag EndEvents#tag Events scanUIDTimer	#tag Event		Sub Action()		  'dim text as string		  'dim number as integer		  '		  'text = chrB(&h01) + chrB(&h0D) + chrB(&h00) + chrB(&h00) + chrB(&h00) + chrB(&h00) + chrB(&h60) + chrB(&h11) + chrB(&h07) + chrB(&h01) + chrB(&h00) + chrB(&h7B) + chrB(&h84)		  '		  'Serial1.Write(text)		  static i as Integer		  dim reader as new tiS6350		  		  select case G_Preferences.hardwareType		  case "CoreSpeedway","CoreSpeedway i166"		    		    reader.invAllSlots(serial1)		    		  case "Kyosho IC"		    //Switched to polling method, polling is enabled when COM port is opened		    //SerialLapCounters1.inv1Slot		  end select		  		  if M_SelectingRacers=true then		    		    i=i+1		    		    if i=100 then		      		      readyToAddRacers=true//next time reader polls now it will add it, this is late time it polls (critical)		      select case G_Preferences.hardwareType		      case "CoreSpeedway","CoreSpeedway i166"		        reader.invAllSlots(serial1)		      case "Kyosho IC"		        //Switched to polling method, polling is enabled when COM port is opened		        //SerialLapCounters1.inv1Slot		      end select		      i=0		      		      M_SelectingRacers=false		      		      //MsgBox "Shut down from timer in select mode"		      me.Mode=0		    end if		    		  else		    i=i+1		    editRacerScanProgressBar.Value=i		    if i>=200 or scanUIDtimerShutDown=true then		      i=0		      editRacerScanProgressBar.Value=0		      scanUIDtimerShutDown=false		      M_EditingRacerScan = false		      me.Mode=0		    end if		  end if		  'hits=hits+1		  'hitsText.Text=str(hits)		End Sub	#tag EndEvent#tag EndEvents#tag Events trackUIDtimer	#tag Event		Sub Action()		  if G_Preferences.hardwareType = "CoreSpeedway" then		    dim reader as new tiS6350		    reader.inv1Slot(serial1)		  elseif G_Preferences.hardwareType = "Kyosho IC" then		    //Switched to polling method, this is no longer needed		    //SerialLapCounters1.inv1Slot		  end if		  		  if SerialLapCounters2.hardwaretype = "CoreSpeedway" then		    SerialLapCounters2.inv1Slot		  elseif SerialLapCounters2.hardwaretype = "Kyosho IC" then		    //Switched to polling method this is no longer needed		    //SerialLapCounters2.inv1Slot		  end if		  		End Sub	#tag EndEvent#tag EndEvents#tag Events SerialLapCounters1	#tag Event		Sub DetectedUIDs(uids as string, times as double)		  dim rs as RecordSet		  dim uid as string		  		  if lenb(UID)>=0 then		    //no need to check database now, only needed if we are editing or selecting racers		    //rs = G_database.SQLSelect("SELECT * FROM racer_table WHERE " + "uid == '" + uids(0) + "'")		    		    //if rs.Field("uid").StringValue=uids(0) then		    		    //Need to add catch here to only do a check if one of the UID's is valid for being checked~		    //If at least one is valid then allow the checks below~		    		    if M_RaceActive=true then//should only register UID's when the race is active~ //ACTIVE RACE		      		      		      if M_GamePaused=false then//should also only register if the game isn't paused!		        		        G_Preferences.theRaceObject.CalculateNewRacerValues(uids,times)		        //No longer needed moved into the above method		        //G_Preferences.theRaceObject.CalculatePlace()//finds who is in first place and sets places accordingly for all racers		        		        //In calculateNewRaceValues it will animate for statistics screen & Race Screen only if minimum lap time has passed for at least one racer.		        //This is to prevent memory leak error inherit in the graphing code~		        		      end if		      		    else//SELECTING OR EDIT RACERS~		      		      if M_SelectingRacers=true then//SELECTING RACERS		        		        //Make sure that the recieved UID matches an inactive racer before proceeding		        for l as integer=0 to UBound(G_Preferences.theRaceObject.racers)		          if G_Preferences.theRaceObject.racers(l).UID = uids and G_Preferences.theRaceObject.racers(l).Active = false and G_Preferences.theRaceObject.racers(l).Enabled = True then		            //We have  match for an inactive racer so we can proceed		            G_Preferences.theRaceObject.AddRacerToRace(uids,l,times)		          end if		        next		        		        		      else//EDIT RACERS		        //Make sure we are actually scanning for a racer		        if M_EditingRacerScan = true then		          //find the racers record		          rs = G_database.SQLSelect("SELECT * FROM racer_table WHERE " + "uid == '" + uids + "' AND enabled == 1")		          if uidField.Text = "none" then		            //just set the UID text field as they want a new racer created		            uidField.Text = uids		          elseif rs.Field("uid").StringValue = uids then		            if scanUIDtimerShutDown=false then//so it only does it once		              scanUIDtimerShutDown=true		            end if		            uidField.Text=""		            // Populate edit racer feilds with results from previous query using method		            M_selectedRecordSet = rs		            carCanvas.Backdrop = Nil		            PopulateEditRacerInfo()//to reflect the change it will fill the editracer info with the item that is selected~		            		            for i as integer=0 to RacerListBox.ListCount-1		              if RacerListBox.cell(i,7)=M_selectedRecordSet.Field("uid").StringValue then		                RacerListBox.Selected(i)=true		              else		                RacerListBox.Selected(i)=false//deselect old selections		              end if		            next		            //In case there is not record update UID field so that we can use it		            uidField.Text=uids		          end if		        end if		      end if		      		    end if		    //else//must be a new racer, clear the fields for him? (might not want to do this, you decide)		    		    //end if		    		    		  end if		  		  //Logic to catch UID and send it to the racer screen plus shut down the timer if there is no record selected		  if M_EditingRacerScan = true then		    scanUIDtimerShutDown = true		    uidField.Text=uids		    M_EditingRacerScan = false		  end if		  		End Sub	#tag EndEvent	#tag Event		Sub ReaderVersion(type as string, version as String)		  select case type		    		  case "CoreSpeedway"		    		    if version = "09" then		      //We have the infinity 166 module, slow down		      trackUIDtimer.Period = 31		    else		      trackUIDtimer.Period = 10		    end if		  end select		End Sub	#tag EndEvent#tag EndEvents#tag Events SerialLapCounters2	#tag Event		Sub DetectedUIDs(uids as string, times as double)		  		  // If a race is running update laps and rank		  if M_RaceActive=true then		    G_Preferences.theRaceObject.CalculateNewRacerValues(uids,times)		    //No longer needed moved into the above method		    //G_Preferences.theRaceObject.CalculatePlace()		  end if		End Sub	#tag EndEvent	#tag Event		Sub ReaderVersion(type as string, version as String)		  select case type		    		  case "CoreSpeedway"		    		    if version = "09" then		      //We have the infinity 166 module, slow down		      trackUIDtimer.Period = 31		    else		      trackUIDtimer.Period = 10		    end if		  end select		End Sub	#tag EndEvent#tag EndEvents#tag Events checkUpdateSocket	#tag Event		Sub PageReceived(url as string, httpStatus as integer, headers as internetHeaders, content as string)		  //We got data from our download of the version file		  //Now we need to check to see what version is available and compare it to what is running		  //Make sure we are supposed to check for updates		  if G_Preferences.readB("checkForUpdates") = true then		    if httpStatus = 200 then		      //Status 200 is good so we can continue processing		      //Convert the received version number to an integer		      dim receivedVersion as integer		      receivedVersion = round(val(content))		      		      //Convert the running version number to an integer		      dim currentVersion as integer		      		      //Make sure to strip off the first 8 characters before conversion		      //dim stringversion as string		      //stringversion = rightb(StaticText_BuildVersion.Text,4)		      //currentVersion = round(val(stringversion)*100)		      currentVersion = App.MajorVersion*100 + App.MinorVersion		      		      //now compare the two if the received version is greater prompt the user otherwise do nothing		      		      if receivedVersion > currentVersion then		        Dim n as Integer		        n = MsgBox(t_newversionavailable,36)		        		        if n=6 then		          //User pressed yes		          ShowURL("www.flipsideracing.org")		          		        ElseIf n=7 then		          //User pressed no		          //Do nothing		        end if		        		        		        		        		        		      end if		    end if		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events endCountDownTimer	#tag Event		Sub Action()		  //If the ending count down feature is enabled this timer will increment each second and announce remaining time		  		  		  if M_endCountDownTime > 0 and G_Preferences.theRaceObject.timeRemaining > 1 then		    //Speak the time remaining and then subtract 1 second for the next iteration of the timer		    robotSpeak(str(round(G_Preferences.theRaceObject.timeRemaining)),true)		    		    M_endCountDownTime = M_endCountDownTime -1		    		  else		    //We should just stop as we have reached the limit		    		    me.Mode = 0		    		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events fsTheme	#tag Event		Sub themeChanged()		  //Logic to apply theme settings is here		  		  //Check to see if we are supposed to scale the background and apply		  if fsTheme.backgroundImage <> Nil then		    if fsTheme.scaleBackground = True then		      mainWindow.Backdrop = scalePicture(fsTheme.backgroundImage, mainWindow.Width, mainWindow.Height,false)		    else		      mainWindow.Backdrop = fsTheme.backgroundImage		    end if		    G_Preferences.backgroundImage = fsTheme.backgroundImage		  end if		  		  //Set main window background color		  mainWindow.BackColor = fsTheme.backgroundColor		  		  //Update images for buttons		  		  dim p as picture		  if fsTheme.mainLogo <> Nil and fsTheme.mainLogo_Mask <> Nil then		    p = New Picture(fsTheme.mainLogo.Width,fsTheme.mainLogo.Height,32)		    p.Graphics.DrawPicture fsTheme.mainLogo,0,0		    p.Mask.Graphics.DrawPicture fsTheme.mainLogo_Mask,0,0		    canvas_M_mainLogo.Backdrop  = p		    canvas_RC_mainLogo.Backdrop = canvas_M_mainLogo.Backdrop		    G_Preferences.mainLogoImage = fsTheme.mainLogo		    G_Preferences.mainLogoImageMask = fsTheme.mainLogo_Mask		  end if		  		  if fsTheme.version < 2 then		    if fsTheme.main_Button <> Nil and fsTheme.main_Mask <> Nil and fsTheme.main_MouseOver <> Nil then		      		      p = New Picture(fsTheme.main_Button.Width,fsTheme.main_Button.Height,32)		      p.Graphics.DrawPictu