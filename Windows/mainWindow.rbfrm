t#tag EndEvents#tag Events Thread_Core166	#tag Event		Sub Run()		  //Make sure we did not trigger the timer too early, this can happen if the last timer period was delayed		  static lastTime as double		  dim currentTime as double		  dim reader as new tiS6350		  while true		    currentTime = Microseconds		    		    //because microseconds are 1/1000000 of a second and me.period is  1/1000		    //we multiply by 800 to allow a variance of down to 80% of the timer period		    if (currentTime - lastTime) > 21000 then		      //We triggered too soon return		      //lastTime = currentTime		      		      		      //Check to see if we have a tag to silence		      if tagToSilence = "" then		        //No tag so we can inventory		        if G_Preferences.hardwareType = "CoreSpeedway i166" then		          reader.inv1Slot(serial1)		          serial1.XmitWait		        end if		        		        if SerialLapCounters2.hardwaretype = "CoreSpeedway i166" then		          SerialLapCounters2.inv1Slot		        end if		      else		        //We have a tag to silence		        reader.silenceTag(tagToSilence,serial1)		        		        //reset the property so on the next loop we inventory		        tagToSilence = ""		      end if		      		      		      		      lastTime = Microseconds		    end if		    me.Sleep(1)		  wend		End Sub	#tag EndEvent#tag EndEvents#tag Events fsEventManager	#tag Event		Sub eventCreated(theName as string)		  eventEditControl1.eventCreated(theName)		End Sub	#tag EndEvent	#tag Event		Sub eventDeleted(theName as String)		  eventEditControl1.eventDeleted(theName)		End Sub	#tag EndEvent	#tag Event		Sub eventLoaded(theName as String)		  eventEditControl1.eventLoaded(theName)		  emControlTree1.eventLoaded(theName)		End Sub	#tag EndEvent	#tag Event		Sub classAdded(name as string, id as string)		  emControlTree1.classAdded(name,id)		End Sub	#tag EndEvent	#tag Event		Sub heatAdded(name as string, id as string)		  emControlTree1.heatAdded(name,id)		End Sub	#tag EndEvent	#tag Event		Sub mainAdded(name as string, id as string)		  emControlTree1.mainAdded(name,id)		End Sub	#tag EndEvent	#tag Event		Sub heatRoundAdded(name as string, id as string)		  emControlTree1.heatRoundAdded(name,id)		End Sub	#tag EndEvent#tag EndEvents