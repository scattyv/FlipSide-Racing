n false		End Function	#tag EndEvent#tag EndEvents#tag Events makePopUpMenu	#tag Event		Sub Change()		  if windowOpened = true then		    dim rs as RecordSet		    dim i as integer		    		    modelPopUpMenu.DeleteAllRows		    		    modelPopUpMenu.AddRow ""		    		    if makePopUpMenu.Text = "none" or makePopUpMenu.Text = "" then		      modelPopUpMenu.ListIndex = 0		    end if		    		    rs = G_Database.SQLSelect("SELECT make_id FROM lu_make WHERE make = '"+Me.Text+"'")		    		    i = rs.Field("make_id").IntegerValue		    rs.close		    		    rs = G_Database.SQLSelect("SELECT * FROM lu_model WHERE fk_make_id = '" + str(i) + "'")		    		    if rs <> nil then		      while rs.EOF = false		        modelPopUpMenu.AddRow rs.Field("model").StringValue		        rs.MoveNext		      wend		      rs.close		    end if		    		    modelPopUpMenu.ListIndex=0		  end if		End Sub	#tag EndEvent	#tag Event		Sub GotFocus()		  dim rs as RecordSet		  		  me.DeleteAllRows		  me.AddRow("")		  rs = G_Database.SQLSelect("SELECT DISTINCT make FROM lu_make ORDER BY make")		  		  while rs.EOF = false		    me.AddRow rs.Field("make").StringValue		    rs.MoveNext		  wend		  rs.close		End Sub	#tag EndEvent	#tag Event		Sub Open()		  dim rs as RecordSet		  		  		  me.DeleteAllRows		  //need to have the option of none		  me.AddRow("")		  		  rs = G_Database.SQLSelect("SELECT DISTINCT make FROM lu_make ORDER BY make")		  		  while rs.EOF = false		    me.AddRow rs.Field("make").StringValue		    rs.MoveNext		  wend		  rs.close		End Sub	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  //If the person has typed an new entry and hit Enter then we want to try inserting it into the database as a new Make that is available		  Static previouskey as string		  if previouskey <> me.text then		    		    dim n as integer		    dim value as string		    		    value = me.Text		    		    		    if Key = chrb(&hD) then		      		      dim query as string		      dim rs as recordset		      dim message as string		      if lenb(Me.Text) > 0 then		        query = "SELECT * FROM lu_make WHERE make  = '"+Me.Text+"'"		        rs = G_Database.SQLSelect(query)		        		        //Make sure there is not a duplicate		        if rs <> Nil then		          if rs.RecordCount > 0 then		            MsgBox "Error, duplicate name"		            Return True		          end if		        end if		        message = replace(t_doyouwanttoinsertintodb, "%t", Me.Text)		        		        query = "INSERT INTO lu_make(make) VALUES ('"+Me.Text+"');"		        G_Database.SQLExecute(query)		        		        n=MsgBox(message,36)		        		        If n=6 then		          //user pressed Yes		          if G_Database.Error then		            MsgBox "Error inserting record, duplicate entry"		          else		            G_Database.Commit		          end if		          		          		          me.DeleteAllRows		          me.AddRow "none"		          		          rs = G_Database.SQLSelect("SELECT DISTINCT make FROM lu_make ORDER BY make")		          		          while rs.EOF = false		            me.AddRow rs.Field("make").StringValue		            rs.MoveNext		          wend		          rs.close		          		          SelectPopUpMenuOption(me, value)		          		        elseif n=7 then		          //user pressed No		          G_Database.Rollback		          		        end if		        		      end if		      		    end if		    		  end if		  		  previouskey = me.text		End Function	#tag EndEvent#tag EndEvents#tag Events modelPopUpMenu	#tag Event		Sub Open()		  me.DeleteAllRows		  me.AddRow ""		End Sub	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  //If the person has typed an new entry and hit Enter then we want to try inserting it into the database as a new Model that is available		  Static previouskey as string		  if previouskey <> me.text then		    di