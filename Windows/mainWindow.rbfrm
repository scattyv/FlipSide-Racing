ue)		          rs.MoveNext		        wend		        		      end if		    end if		  end if		  		  //Now set the uid back so the record looks correct		  uidField.Text = uid		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub ReadShellBuffer(buffer as string)		  //read data coming from shell command and send results to calculate new racer values		  dim first as integer		  dim n as Integer		  dim record as string		  dim idstring as string		  dim timestring as string		  dim didtimestring as double		  dim didstring as double		  		  		  first = instrb(buffer, "@")		  		  if first = 0 then		    return		  else		    		    n = Lenb(buffer)		    buffer=buffer.rightb(n-first+1)		    		    if n < 26 then		      return		    else		      record = buffer.leftb(26)		      M_shellbuffer = buffer.rightb(n-26)		      idstring = Record.midb(2,16)		      timestring = record.midb(19,8)		    end if		  end if		  		  		  didstring = val("&h"+idstring)		  didtimestring = (cdbl(timestring)/100)		  msgbox str(didstring)		  msgbox str(didtimestring)		  		  //fixme need to use time from external commands		  G_Preferences.theRaceObject.CalculateNewRacerValues(str(didstring),0)		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub RearrangeEditRacerScreen()		  //This method is used to show, hide and rearrange the controls on the edit racer screen and to show or hide columns of the table on the screen		  		  dim offset as integer		  dim totaloffset as integer		  offset = 30		  //Where does the first control end up, this starts as the offset just past the enabled option		  totaloffset = 280		  		  //Resize column widths according to which columns are enabled		  RacerListBox.ColumnWidths = "0,1.5*,"+G_Preferences.readS("editRacersClass")+","+G_Preferences.readS("editRacersChassis")+","+G_Preferences.readS("editRacersMake")+","+G_Preferences.readS("editRacersModel")+_		  ","+G_Preferences.readS("editRacersMotor")+",1.5*,"+G_Prefer