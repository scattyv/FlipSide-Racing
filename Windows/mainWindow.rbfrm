integer		  		  		  if G_Preferences.theRaceObject.RaceType = "Free Play" or G_Preferences.theRaceObject.roundType = "Practice" and M_RaceActive = true then		    //We are in a race, we can proceed		    //We need to go through each active racer and see if we find a match to what was clicked		    for i as integer = 0 to UBound(activeIndexes)		      rank = val(RankText(index).Text)		      		      if M_Slots(activeIndexes(i)).currentPlace = rank and M_Slots(activeIndexes(i)).theName = NameRankText(index).Text then		        //We have a match, exterminate		        		        RemoveRacerSlot(activeIndexes(i))		        //Now we bail, otherwise it will delete everyone after this because all the slots just shifted		        return		        		      end if		      		      		    next		    		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub RemoveRacerSlot(index as integer)		  G_Preferences.theRaceObject.slotsEnabled(index) = False		  		  //Deactivate racer		  for i as integer = 0 to UBound(G_Preferences.theRaceObject.racers)		    if G_Preferences.theRaceObject.racers(i).RacerID = M_Slots(index).RacerID then		      G_Preferences.theRaceObject.racers(i).Active = False		      exit for i		    end if		  next		  		  for i as integer = 0 to UBound(G_Preferences.theRaceObject.activeRacers)		    if G_Preferences.theRaceObject.activeRacers(i).RacerID = M_Slots(index).RacerID then		      G_Preferences.theRaceObject.activeRacers.Remove(i)		      exit for i		    end if		  next		  		  M_Slots(index).Active=false//as a test to see if it copied by reference or made a copy.  If it shows up again from the selection then I know it's by reference		  M_Slots(index)=nil//nullify it now since it's supposed to have nothing, to represent no racer in the slot		  		  clearRaceScreen		  AnimateRaceScreen		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub ResetBestLap(index as integer)		  //gets the indexes for the M_Slots that are active		  dim activeIndexes(-1) as integer = FindActiveIndexes()		  dim rank as integer		  		  		  if G_Preferences.theRaceObject.RaceType = "Free Play" or G_Preferences.theRaceObject.roundType = "Practice" and M_RaceActive = true then		    //We are in a race, we can proceed		    //We need to go through each active racer and see if we find a match to what was clicked		    for i as integer = 0 to UBound(activeIndexes)		      rank = val(RankText(index).Text)		      		      if M_Slots(activeIndexes(i)).currentPlace = rank and M_Slots(activeIndexes(i)).theName = NameRankText(index).Text then		        //We have a match, reset best lap		        		        M_Slots(activeIndexes(i)).best_time = 999		        G_Preferences.theRaceObject.ResetBestLap(M_Slots(activeIndexes(i)).RacerID)		        //refresh the screen to show the update		        clearRaceScreen		        AnimateRaceScreen		        //Now we bail, otherwise we are doing extra work		        return		        		      end if		      		      		    next		    		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub ResizeLinuxControls()		  		  // Linux controls have the text cut off by default so for Linux we are going to set the new control height to 28		  // placeholder, update this to be dynamic for resizing of windows		  		  Const kNewHeight = 28		  Dim i as Integer		  Dim ctl as RectControl		  		  // loop through all of the controls on the window		  For i = 0 to controlCount - 1		    // We can only resize RectControls so we can skip any control that is not one		    If NOT control( i ) isa RectControl then		      continue // move to the next control in the loop		    End if		    		    // The Window.Control array will return an object of the type Control.		    // The Control class does not have the height property that we are interested		    // in, but the RectControl class does.  In order to go from a Control to a		    // RectControl we need to use typecasting.  After we have the object as a		    // RectControl we can access the Height property.		    ctl = RectControl( control(i) )		    		    // The first criteria to determine if a control needs to be resized is to		    // check and see if the control height is below the new height.		    // Only controls that are smaller than the new height will have to be adjusted.		    If ctl.height < kNewHeight then		      		      // The next requirement is that the control is a type of control that we		      // want to resize. This includes PushButton, StaticText, EditField,		      // PopupMenu, and ComboBox.		      If (ctl isa PushButton) OR (ctl isa StaticText) OR (ctl isa EditField) OR _		        (ctl isa PopupMenu) OR (ctl isa ComboBox) then		        // The control meets all of the criteria for resizing, so now		        // we can set the height.		        ctl.height = kNewHeight		        		      End if		      		    End if		  Next		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub robotSpeak(text as string, interrupt as boolean)		  If G_Preferences.useAudio = True then		    		    //ThreadPoolManager.Queue(new Speech(text, interrupt))		    //speak(text, interrupt)		    		    //If interrupt is set to true put the text at the beginning of the queue		    if interrupt = true then		      TTSThread1.Insert(text, interrupt)		    else		      //If interrupt is false append it to the end of the queue		      TTSThread1.Append(text, interrupt)		    end if		    		    //Check to see if thread is running, if not start it		    if TTSThread1.State = 4 then		      TTSThread1.Run		    end if		    		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub SaveRacerInfo()		  if M_selectedRecordSet <> Nil then		    		    //Now you just got to know which 'race car' it was in the control structure that was being edited.		    dim i as Integer		    for i=0 to UBound(G_Preferences.theRaceObject.racers)		      if G_Preferences.theRaceObject.racers(i).theName=M_selectedRecordSet.Field("name").Value and G_Preferences.theRaceObject.racers(i).UID=M_selectedRecordSet.Field("uid").StringValue then		        //this will force the changes in the control structure to reflect also not just the DB.		        G_Preferences.theRaceObject.racers(i).theName=nameField.Text		        //check to see if a seperate name pronunciation is included, if not just use the name field		        if pronounceNameField.Text = "" or pronounceNameField.Text = chrb(&h00) then		          G_Preferences.theRaceObject.racers(i).speakName=nameField.Text		        else		          G_Preferences.theRaceObject.racers(i).speakName=pronounceNameField.Text		        end if		        G_Preferences.theRaceObject.racers(i).UID=uidField.Text		        G_Preferences.theRaceObject.racers(i).theChassis=chassisPopUpMenu.Text		        G_Preferences.theRaceObject.racers(i).theClass=classPopUpMenu.Text		        G_Preferences.theRaceObject.racers(i).theMake=makePopUpMenu.Text		        G_Preferences.theRaceObject.racers(i).theModel=modelPopUpMenu.Text		        G_Preferences.theRaceObject.racers(i).theMotor=MotorPopUpMenu.Text		        G_Preferences.theRaceObject.racers(i).theCrystal=PopUpMenu_ER_Crystal.Text		        G_Preferences.theRaceObject.racers(i).theScale=scalePopUpMenu.Text		        G_Preferences.theRaceObject.racers(i).theImage=M_tempCarImage		        G_Preferences.theRaceObject.racers(i).theImagePath=M_tempCarImagePath		        G_Preferences.theRaceObject.racers(i).theImageMask=M_tempCarMask		        G_Preferences.theRaceObject.racers(i).theImageMaskPath=M_tempCarMaskPath		        G_Preferences.theRaceObject.racers(i).theEmail=EditField_ER_Email.Text		        G_Preferences.theRaceObject.racers(i).theColor = Rectangle_ER_Color.FillColor		        //If this racer is enabled we first must disable any matching UID to prevent duplicate UID's from being enabled		        if CheckBox_ER_Enabled.Value = True then		          for j as integer = 0 to Ubound(G_Preferences.theRaceObject.racers)		            if G_Preferences.theRaceObject.racers(j).UID = uidField.Text then		              G_Preferences.theRaceObject.racers(j).Enabled = False		            end if		          next		          //We must also disable it in the database		          G_Database.SQLExecute("UPDATE racer_table SET enabled = 0 WHERE uid == '" + uidField.Text + "'")		        end if		        G_Preferences.theRaceObject.racers(i).Enabled=CheckBox_ER_Enabled.Value		        		        		        		        exit//we found what we wanted to edit, so leave the loop~		      end if		    next		    		    //Now reflect changes in the database accordingly~		    M_selectedRecordSet.Edit		    		    M_selectedRecordSet.Field("name").Value = nameField.Text		    //Check to see if they entered a seperate pronounciation		    if pronounceNameField.Text = chrb(&h00) then		      M_selectedRecordSet.Field("name_pronounce").Value = ""		    else		      M_selectedRecordSet.Field("name_pronounce").Value = pronounceNameField.Text		    end if		    M_selectedRecordSet.Field("uid").Value = uidField.Text		    M_selectedRecordSet.Field("scale").Value = scalePopUpMenu.Text		    M_selectedRecordSet.Field("chassis").Value = chassisPopUpMenu.Text		    M_selectedRecordSet.Field("class").Value = classPopUpMenu.Text		    		    //if makePopUpMenu.Text <> "" then		    M_selectedRecordSet.Field("make").Value = makePopUpMenu.Text		    //else		    //M_selectedRecordSet.Field("make").Value = "none"		    //end if		    		    //if modelPopUpMenu.Text <> "" then		    M_selectedRecordSet.Field("model").Value = modelPopUpMenu.Text		    //else		    //M_selectedRecordSet.Field("model").Value = "none"		    //end if		    M_selectedRecordSet.Field("motor").Value = MotorPopUpMenu.Text		    		    //if PopUpMenu_ER_Crystal.Text <> "" then		    M_selectedRecordSet.Field("crystal").Value = PopUpMenu_ER_Crystal.Text		    //else		    //M_selectedRecordSet.