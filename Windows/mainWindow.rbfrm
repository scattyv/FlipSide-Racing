eed to make sure to disable lap counter polling is disabled if application is closed		    SerialLapCounters1.polling(false)		    SerialLapCounters2.polling(false)		    SerialLapCounters1.XmitWait		    SerialLapCounters2.XmitWait		  end if		  		  //Check the Window location and save it so the next time we open it will be in the same place		  G_Preferences.write("windowTop",mainWindow.Top)		  G_Preferences.write("windowLeft",mainWindow.Left)		  G_Preferences.write("windowHeight",mainWindow.Height)		  G_Preferences.write("windowWidth",mainWindow.Width)		  		  		  		  		  //Force quit of application		  Quit		End Sub	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  if G_Debug=true then		    if M_RaceActive=true then//should only register UID's when the race is active~		      if M_GamePaused=false then//should also only register if the game isn't paused!		        dim uid as string = key		        G_Preferences.theRaceObject.CalculateNewRacerValues(uid,0)		        //G_Preferences.theRaceObject.CalculatePlace()//finds who is in first place and sets places accordingly for all racers		        //AnimateRaceScreen()		        if G_Preferences.readB("disableLiveGraphing")=false then		          AnimateStatisticScreen()		          statisticsCanvas.Draw		        end if		      end if		    end if		  else		    //MsgBox "Debug mode is off, so the numbers won't work"		  end if		  		  //If we are not in keyboard mode then allow hot keys for managing race		  if G_Preferences.hardwareType <> "Keyboard" then		    select case key		      		    case "r"		      //Announce ranks if the key is r		      robotSpeak(G_Preferences.theRaceObject.LiveRankString, false)		    case "s"		      //Start or stop the race		      if M_RaceActive = true then		        //Race is running so end it		        EndTheRace()		      else		        //Race is not running so start it		        startOrStopRace()		      end if		    case "p"		      //Pause the race, or start if not started		      startOrStopRace()		    case "e"		      //End the race		      EndTheRace()		    case "q"		      //Make it be quiet		      robotSpeak(" ",True)		    case "m"		      //JDA Fixme, debug to get to event manager temporarily		      PagePanel1.Value=7		    end select		    		  end if		End Function	#tag EndEvent	#tag Event		Sub Maximize()		  G_Preferences.write("windowMaximize",True)		  me.Refresh		End Sub	#tag EndEvent	#tag Event		Sub Open()		  //Add the full screen option for Mac OS X Lion		  self.setCollectionBehavior = NSWindowCollectionBehavior.FullScreenPrimary		  		  //Build version number and display it		  dim versionNum as double		  versionNum = app.MajorVersion + app.MinorVersion/100		  		  version = format(versionNum,"#.0#")		  //version = str(app.MajorVersion)+"."+str(app.NonReleaseVersion)		  if app.StageCode < 3 then		    StaticText_BuildVersion.Text = t_betaversion + " " + version		  else		    StaticText_BuildVersion.Text = t_version + " " + version		  end if		  		  GM_LogToSplash("Main Window Initialized")		  		  //Initialize the theme class		  //dim fsTheme as new theme(G_Database)		  fsTheme.themeDB = G_Database		  		  		  mainWindow.SetFocus//fixes keydown bug ::WORKAROUND::		  mainWindow.Title = G_Preferences.readS("clubName")		  		  GM_LogToSplash("Setting Theme")		  //If a proper theme has been chosen set it		  if G_Preferences.readS("themeName") <> "" then		    		    fsTheme.set(G_Preferences.readS("themeName"))		  end if		  		  //Create race screen controls		  GM_LogToSplash("Create Race Screen Controls")		  createRaceScreenControls		  		  		  GM_LogToSplash("Main Window Set Defaults")		  		  //Set defaults		  mainwindow.SetDefaults()		  		  GM_LogToSplash("Main Window Initialize TTS")		  //Send blank audio to TTS so it loads libraries		  If G_Preferences.useAudio = True then		    mainWindow.robotSpeak(" ",false)		    If AudioThread1.State = 4 then		      AudioThread1.Run		    end if		  end if		  		  GM_LogToSplash("Adjusting Max Racers And Row Spacing")		  ChangeMaxRacers		  rowSpacing = G_Preferences.rowSpacing		  ChangeRacerRowSpacing(rowSpacing)		  		  		  GM_LogToSplash("Divide By Zero")		  		  //Check previous Window position and size and use that if available		  if G_Preferences.readB("windowMaximize") = True then		    //The user last maximized the window		    //Set the top and left parameters just in case they have multiple monitors attached so it will maximize in the correct screen		    mainWindow.Top = G_Preferences.readI("windowTop")		    mainWindow.Left = G_Preferences.readI("windowLeft")		    mainWindow.Maximize		  else		    		    if G_Preferences.readI("windowWidth") > 1023 and G_Preferences.readI("windowHeight") > 589 then		      //We have a valid size, use it		      mainWindow.Width = G_Preferences.readI("windowWidth")		      mainWindow.Height = G_Preferences.readI("windowHeight")		      //Set the location of the window based on the previous location		      mainWindow.Top = G_Preferences.readI("windowTop")		      mainWindow.Left = G_Preferences.readI("windowLeft")		    else		      //Just maximize the Window as we have inconsistenly defined parameters or defaults		      mainWindow.Maximize		    end if		  end if		  		  //Check to see if they want the window size the same as the background image		  GM_LogToSplash("Set Background")		  if fsTheme.backgroundImage <> Nil then		    if fsTheme.scaleBackground <> True then		      if fsTheme.backgroundImage.Width < mainWindow.Width and fsTheme.backgroundImage.Width > 1023 then		        mainWindow.Width = fsTheme.backgroundImage.Width		      end if		      		      		      if fsTheme.backgroundImage.Height < mainWindow.Height and fsTheme.backgroundImage.Height > 589 then		        mainWindow.Height = fsTheme.backgroundImage.Height		      end if		    end if		  end if		  		  //Pass database to the event manager so it can work		  fsEventManager.initialize(G_Database)		  		  //Initiate update check		  checkVersion		  //Check the pageprocessed event for data		  		  //Close the splash screen		  splashScreen.Close		  		  //Set the window opened property to true otherwise change events for controls will not trigger		  windowOpened = True		End Sub	#tag EndEvent	#tag Event		Sub Resized()		  //Check to see if we are supposed to scale the background and apply		  if fsTheme.backgroundImage <> Nil then		    if fsTheme.scaleBackground = True then		      mainWindow.Backdrop = scalePicture(fsTheme.backgroundImage, mainWindow.Width, mainWindow.Height,false)		    else		      mainWindow.Backdrop = fsTheme.backgroundImage		    end if		  end if		  		  //Check height and change controls if needed		  if me.Height < 720 then		    canvas_M_mainLogo.Visible = False		    canvas_RC_mainLogo.Visible = False		    		  else		    canvas_M_mainLogo.Visible = True		    canvas_RC_mainLogo.Visible = True		    		  end if		  		  		  //Rearrange controls on the general Config screen if we have a really low res display		  if me.Height < 600 then		    GroupBox_GC_Clubs.Top = 92		    GroupBox_GC_GeneralSettings.Top = 92		    GroupBox_GC_GUI.Top = 92		    GroupBox_GC_Hardware.Top = 330		    GroupBox_GC_Database.Top = 330		    GroupBox_GC_Email.Top = 330		    		  else		    GroupBox_GC_Clubs.Top = 100		    GroupBox_GC_GeneralSettings.Top = 100		    GroupBox_GC_GUI.Top = 100		    GroupBox_GC_Hardware.Top = 350		    GroupBox_GC_Database.Top = 350		    GroupBox_GC_Email.Top = 350		    		  end if		  		  		  		  me.Refresh		  		  		End Sub	#tag EndEvent	#tag Event		Sub Restore()		  G_Preferences.write("windowMaximize",False)		  me.Refresh		End Sub	#tag EndEvent	#tag Method, Flags = &h21		Private Sub AlterEventName()		  //Alter Event Name V.1.0		  		  'Description		  '		  'The Purpose of this method is so that if the auto name checkbox was enabled for the event name to be automatically generated based on the settings given so that you can just start another		  'race right away without hassle of the database not allowing the 'event_name' because it's unique.		  		  //Create a string based on the known factors about this event.		  		  //cleanup		  //dim newEventName as string		  dim d as new date		  		  if lenb(mainWindow.eventNameField.Text) > 0 then		    G_Preferences.theRaceObject.eventName = mainWindow.eventNameField.Text +" "+ mainWindow.roundTypePopUpMenu.Text+" "+d.SQLDateTime		  else		    G_Preferences.theRaceObject.eventName = mainWindow.roundTypePopUpMenu.Text+" "+d.SQLDateTime		  end if		  // Leave eventName field alone so that users can have unique names along with the autogenerated info		  //cleanup		  //mainWindow.eventNameField.Text = newEventName		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub AnimateFromIndex(index as integer, mSlotIndex as integer)		  dim lapTime as double = 0		  dim i as integer		  dim racerColor as color		  		  if G_Preferences.readS("editRacersColor") <> "*" then		    racerColor = M_Slots(mSlotIndex).textColor		  else		    racerColor = M_Slots(mSlotIndex).theColor		  end if		  		  //Check to see if this is visible, if not make it so		  if RankText(index).Visible = False then		    RankText(index).Visible = True		    RcCanvas(index).Visible = True		    NameRankText(index).Visible = True		    LapsRankText(index).Visible = True		    LapTimeText(index).Visible = True		    //DB_RS_TIME(index).Visible = True		    lastLapRankText(index).Visible = True		    PageRankText(index).Visible = True		    BestLapRankText(index).Visible = True		  end if		  		  		  RankText(index).Text=cstr(M_Slots(mSlotIndex).currentPlace)		  RankText(index).TextColor=racerColor		  		  NameRankText(index).Text=M_Slots(mSlotIndex).theName		  NameRankText(index).TextColor=racerColor		  		  LapsRankText(index).Text=cstr(M_Slots(mSlotIndex).lapCount)		  LapsRankText(index).TextColor=racerColor		  		  i = UBound(M_Slots(mSlotIndex).lapTimes)		  		  //Make sure we even have a lap time to look at		  if i > -1 then		    //Check to see if the race is a staggered start, if it is not then display the last lap time		    //Staggered start races use the lap time column as each racers individual time remaining through the race timer		    if G_Preferences.theRaceObject.RaceType <> "Staggered Time" then		      //Format text in DD:HH:MM:SS.SS format		      //LapTimeText(index).Text=Format(M_Slots(mSlotIndex).lapTimes(i),G_Preferences.lapTimeFormat)		      LapTimeText(index).Text=secondsToTime(M_Slots(mSlotIndex).lapTimes(i), G_Preferences.lapTimeFormat)		    elseif M_Slots(mSlotIndex).raceEnded = True then		      //If it is staggered time and this racer has finished their race we want to show the final time		      LapTimeText(index).Text=secondsToTime(M_Slots(mSlotIndex).lapTimes(i), G_Preferences.lapTimeFormat)		    end if		  end if		  		  LapTimeText(index).TextColor=racerColor		  //Refresh the double buffer canvas to actually display the time		  //DB_RS_TIME(index).Refresh		  		  //Fixme, lag not calculated correctly		  //lagRankText(index).Text=cstr(M_Slots(mSlotIndex).lagTime)		  //lagRankText(index).TextColor=M_Slots(mSlotIndex).textColor		  		  lastLapRankText(index).Text=Format(lapTime,G_Preferences.lapTimeFormat)		  lastLapRankText(index).TextColor=racerColor		  		  BestLapRankText(index).Text=format(M_Slots(mSlotIndex).best_time,G_Preferences.lapTimeFormat)		  BestLapRankText(index).TextColor=racerColor		  		  RcCanvas(index).Backdrop=M_Slots(mSlotIndex).theImage		  		  if G_Preferences.theRaceObject.RaceType <> "Free Play" and G_Preferences.theRaceObject.RaceType <> "Drag" then		    PageRankText(index).Text=Format(M_Slots(mSlotIndex).Pace,"0")		    PageRankText(index).TextColor=racerColor		  else		    PageRankText(index).Text=Format(M_Slots(mSlotIndex).Pace,G_Preferences.lapTimeFormat)		    PageRankText(index).TextColor=racerColor		  end if		  		  		  lapTime = M_Slots(mSlotIndex).lastLapTime		  		  if M_Slots(mSlotIndex).raceEnded = False then		    lastLapRankText(index).Text = Format(lapTime,G_Preferences.lapTimeFormat)		    lastLapRankText(index).TextColor = racerColor		  else		    lastLapRankText(index).Text = t_done		    lastLapRankText(index).TextColor = racerColor		  end if		  		  		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub AnimateRaceScreen()		  dim numRacers as Integer		  		  if PagePanel1.Value <> 4 then		    exit		  end if		  		  //Global Function for this window, since it's called by the threader~		  mainWindow.SetFocus//fixes keydown bug ::WORKAROUND::		  		  //Check to see if all the racers will fit on the window, if not change row spacing		  //if rowSpacing > -16 then		  numRacers = ubound(G_Preferences.theRaceObject.activeRacers) + 1		  //Remove line below because we want to change row space both if it goes up or down		  //if numRacers*(40+rowSpacing)+Separator_RS.Top >me.height then		  dim tempHeight as integer		  		  tempHeight = (me.height - Separator_RS.Top)/numRacers - 41		  if tempHeight < -16 then		    tempHeight = -16		  elseif tempHeight > G_Preferences.readI("rowSpacing") then		    tempHeight = G_Preferences.readI("rowSpacing")		  end if		  		  if rowSpacing <> tempHeight then		    		    rowSpacing = tempHeight		    ChangeRacerRowSpacing(rowSpacing)		  end if		  //end if		  //end if		  		  //Now we need to get which slots are active and the rank that person is in		  dim slots() as integer		  dim ranks() as integer		  for i as integer = 0 to UBound(M_Slots)		    if M_Slots(i)<>nil then		      slots.Append(i)		      ranks.Append(M_Slots(i).currentPlace)		    end if		  next		  		  //If we have entries to animate then sort and animate		  ranks.Sortwith(slots)		  for i as integer = 0 to Ubound(slots)		    AnimateFromIndex(i,slots(i))		  next		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub AnimateStatisticScreen()		  //Check to make sure live graphing is enabled.		  if G_Preferences.readB("disableLiveGraphing")=false then		    //Check to make sure we are on the statistics page, if not then we do not need to update graph		    if PagePanel1.Value = 5 then		      if M_graphTypeSelected="Laps Versus Time" then		        GM_AnimateStatisticsCanvas(G_Preferences.theRaceObject.activeRacers,t_lapsversustime,t_time + " (" + t_seconds + ")",t_laps,statisticsCanvas,"Time_Graph")		      elseif M_graphTypeSelected="Laps Versus Lag" then		        GM_AnimateStatisticsCanvas(G_Preferences.theRaceObject.activeRacers,t_lapsversuslag,t_laps,t_lag + " (" + t_seconds + ")",statisticsCanvas,"Lag_Graph")		      elseif M_graphTypeSelected="Time Per Lap" then//for live animating in this case, we will use a reference to the last added UID for drawing singles		        //M_graphTypeSelected="Laps Versus Time"//just change it to this, can't do time per lap during live race.		        //PopUpMenu_ST_GraphType.ListIndex=0		        //GM_AnimateStatisticsCanvas(G_Preferences.theRaceObject.activeRacers,M_graphTypeSelected,"Time (seconds)","Laps",statisticsCanvas,"Time_Graph")		        GM_AnimateStatisticsCanvas(G_Preferences.theRaceObject.activeRacers,t_timeperlap,t_time + " (" + t_seconds + ")",t_laps,statisticsCanvas,"Time_Per_Lap")		      elseif M_graphTypeSelected="Rank Versus Time" then		        GM_AnimateStatisticsCanvas(G_Preferences.theRaceObject.activeRacers,t_rankversustime,t_time + " (" + t_seconds + ")",t_rank,statisticsCanvas,"Rank_Time")		      else		        MsgBox "Graph type for animating live race not recognized."		      end if		    end if		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub ChangeMaxRacers()		  //This method is used to change the maximum number of racers allowed in a race		  return		  		  dim i as integer		  		  for i = 0 to 49		    if i < G_Preferences.maxRacers then		      //These need to be visible		      		      //Set the row on the race screen to visible		      RankText(i).Visible = True		      RcCanvas(i).Visible = True		      NameRankText(i).Visible = True		      LapsRankText(i).Visible = True		      //Replaced with double buffer canvas		      LapTimeText(i).Visible = True		      ////DB_RS_TIME(i).Visible = True		      lastLapRankText(i).Visible = True		      PageRankText(i).Visible = True		      BestLapRankText(i).Visible = True		      		      		    else		      //These should not be visible		      		      //Set the row on the race screen to not visible		      RankText(i).Visible = False		      RcCanvas(i).Visible = False		      NameRankText(i).Visible = False		      LapsRankText(i).Visible = False		      //Replaced with double buffer canvas		      LapTimeText(i).Visible = False		      ////DB_RS_TIME(i).Visible = False		      lastLapRankText(i).Visible = False		      PageRankText(i).Visible = False		      BestLapRankText(i).Visible = False		      		    end if		  next		  		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub ChangeRacerRowSpacing(spacing as integer)		  //Change spacing on the select racers and race screens		  //Read the global variable rowSpacing to determine spacing		  dim i as integer		  dim rowyloc as integer		  dim top as integer		  		  //If we are going to overlap the header then we need to compensate for that		  if spacing < 0 then		    top = Separator_RS.Top + 1 + spacing * -1		  else		    top = Separator_RS.Top + 1		  end if		  		  //Go through each row and move it		  //Make sure to set the left property also so it is in the correct column, use the first one as a reference		  //Do this for all racer slots not just to max, this value currently needs to be 49		  for i = 0 to 49		    		    		    //Rearrange race screen		    rowyloc = top + spacing + i*(spacing + 40)		    		    RankText(i).Top = rowyloc		    RcCanvas(i).Top = rowyloc		    NameRankText(i).Top = rowyloc		    LapsRankText(i).Top = rowyloc		    //Replace the lap time text with the double buffer control		    LapTimeText(i).Top = rowyloc		    ////DB_RS_TIME(i).Top = rowyloc + 6		    lastLapRankText(i).Top = rowyloc		    PageRankText(i).Top = rowyloc		    BestLapRankText(i).Top = rowyloc		    		    //Resize controls if the spacing is less than 0		    if G_Preferences.rowSpacing < 0 then		      RcCanvas(i).Height = 40 + spacing		      LapTimeText(i).Height = 40 + spacing		      ////DB_RS_TIME(i).Height = 40 + spacing		      RankText(i).Height = 40 + spacing		      NameRankText(i).Height = 40 + spacing		      LapsRankText(i).Height = 40 + spacing		      lastLapRankText(i).Height = 40 + spacing		      PageRankText(i).Height = 40 + spacing		      BestLapRankText(i).Height = 40 + spacing		    else		      RcCanvas(i).Height = 40		      //DB_RS_TIME(i).Height = 24		      RankText(i).Height = 40		      NameRankText(i).Height = 40		      LapsRankText(i).Height = 40		      lastLapRankText(i).Height = 40		      PageRankText(i).Height = 40		      BestLapRankText(i).Height = 40		      		      		      		    end if		    		    //Set left property so they are in the correct column		    RankText(i).Left = RankText(0).Left		    RcCanvas(i).Left = RcCanvas(0).Left		    NameRankText(i).Left = NameRankText(0).Left		    LapsRankText(i).Left = LapsRankText(0).Left		    //Replace the lap time text with the double buffer control		    LapTimeText(i).Left = LapTimeText(0).Left		    //DB_RS_TIME(i).Left = //DB_RS_TIME(0).Left		    lastLapRankText(i).Left = lastLapRankText(0).Left		    PageRankText(i).Left = PageRankText(0).Left		    BestLapRankText(i).Left = BestLapRankText(0).Left		    		    //Set the parent control to the page panel		    RankText(i).Parent = PagePanel1		    RcCanvas(i).Parent = PagePanel1		    NameRankText(i).Parent = PagePanel1		    LapsRankText(i).Parent = PagePanel1		    //Replace the lap time text with the double buffer control		    LapTimeText(i).Parent = PagePanel1		    //DB_RS_TIME(i).Parent = PagePanel1		    lastLapRankText(i).Parent = PagePanel1		    PageRankText(i).Parent = PagePanel1		    BestLapRankText(i).Parent = PagePanel1		    		    //Set the panel index so they show up on the correct PagePanel		    RankText(i).PanelIndex = 4		    RcCanvas(i).PanelIndex = 4		    NameRankText(i).PanelIndex = 4		    LapsRankText(i).PanelIndex = 4		    //Replace the lap time text with the double buffer control		    LapTimeText(i).PanelIndex = 4		    //DB_RS_TIME(i).PanelIndex = 4		    lastLapRankText(i).PanelIndex = 4		    PageRankText(i).PanelIndex = 4		    BestLapRankText(i).PanelIndex = 4		    		  next		  		  //Refresh Window as MS Windows will have artifacts when moving controls		  me.Refresh		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub checkVersion()		  dim urlString as string		  dim osString as string		  dim racersString as string		  dim racesString as string		  dim rs as RecordSet		  dim formData as new dictionary		  		  //Update splash screen		  GM_LogToSplash("Check for Updates")		  		  osString = GM_GetOSVersion		  		  //Find the number of racers		  rs = G_Database.SQLSelect("SELECT COUNT(*) FROM racer_table")		  if rs<>nil then		    racersString = rs.Field("COUNT(*)").StringValue		  end if		  		  //Find the number of races		  rs = G_Database.SQLSelect("SELECT COUNT(*) FROM race_table")		  if rs<>nil then		    racesString = rs.Field("COUNT(*)").StringValue		  end if		  		  		  		  //Next we attempt to send this to the site so it can be diagnosed		  //Build up the form data		  formData.Value("version")=str(App.MajorVersion*100+App.MinorVersion)		  formData.Value("clubname")=G_Preferences.readS("clubName")		  formData.Value("lapcounter")=G_Preferences.readS("hardwareType")		  formData.Value("os")=osString		  formData.Value("racers"