er_ids(i)		      exit		    end if		  next		  		  //We need to know the best lap to do some highlighting on it so we grab it here.		  dim rs as RecordSet		  dim best_lap as Integer		  rs = G_Database.SQLSelect("SELECT * FROM summary_table WHERE event_id=='"+event_id+"' AND racer_id=='"+racer_id+"';")		  if rs<>nil then		    best_lap=rs.Field("best_lap").IntegerValue		  else		    MsgBox "Error finding best_lap info from summary_table"		  end if		  		  //Get Lap information for that racer and store to the temp racer~		  rs = G_Database.SQLSelect("SELECT * FROM laps_table WHERE event_id=='"+event_id+"' AND racer_id=='"+racer_id+"';")		  		  dim racers(-1) as raceCar//all though in this case there will only be '1'		  dim temp as new raceCar		  dim lap_number as Double		  dim k as Integer = 0		  		  temp.theName=getRacerName(racer_id)		  		  ListBox_ST_IndividualRacerTable.DeleteAllRows		  		  while rs.EOF=false//read in the data to create the racer info~		    lap_number = rs.Field("lap_number").DoubleValue		    if lap_number<>0 then		      temp.lapCounts.Append lap_number		      temp.lapTimes.Append rs.Field("lap_time").DoubleValue		      temp.lagTimes.Append rs.Field("lag_time").DoubleValue		      temp.timeAtLap.Append rs.Field("time_at_lap").DoubleValue		      temp.lapPlaces.Append rs.Field("lap_place").IntegerValue		      temp.textColor = G_Preferences.slotColors(0)		      		      ListBox_ST_IndividualRacerTable.AddRow ""//just so it's blank for the start.		      		      if lap_number=best_lap then		        ListBox_ST_IndividualRacerTable.CellBold(k,0)=true		        ListBox_ST_IndividualRacerTable.CellBold(k,1)=true		        ListBox_ST_IndividualRacerTable.CellBold(k,2)=true		      end if		      		      ListBox_ST_IndividualRacerTable.cell(k,0)=temp.theName		      ListBox_ST_IndividualRacerTable.cell(k,1)=cstr(temp.lapCounts(UBound(temp.lapCounts)))		      if M_graphTypeSelected="Laps Versus Time" then		        ListBox_ST_IndividualRacerTable.Heading(-1) = "Racer Name" + Chrb(9) + "Lap" + Chrb(9) + "Time"		        ListBox_ST_IndividualRacerTable.cell(k,2)=cstr(temp.lapTimes(UBound(temp.lapTimes)))		      elseif M_graphTypeSelected="Laps Versus Lag" then		        ListBox_ST_IndividualRacerTable.Heading(-1) = "Racer Name" + Chrb(9) + "Lap" + Chrb(9) + "Lag"		        ListBox_ST_IndividualRacerTable.cell(k,2)=cstr(temp.lagTimes(UBound(temp.lagTimes)))		      elseif M_graphTypeSelected="Time Per Lap" then		        ListBox_ST_IndividualRacerTable.Heading(-1) = "Racer Name" + Chrb(9) + "Lap" + Chrb(9) + "Time"		        ListBox_ST_IndividualRacerTable.cell(k,2)=cstr(temp.timeAtLap(UBound(temp.timeAtLap)))		      elseif M_graphTypeSelected="Rank Versus Time" then		        ListBox_ST_IndividualRacerTable.Heading(-1) = "Racer Name" + Chrb(9) + "Lap" + Chrb(9) + "Rank"		        ListBox_ST_IndividualRacerTable.cell(k,2)=cstr(temp.lapPlaces(UBound(temp.lapPlaces)))		      end if		      		      k=k+1		    end if		    rs.MoveNext		  wend		  		  //No longer needed to remove lap 0		  //if UBound(temp.lapCounts) > 1 then		  //temp.lapCounts.Remove(0)		  //temp.lapTimes.Remove(0)		  //temp.lagTimes.Remove(0)		  //temp.timeAtLap.Remove(0)		  //end if		  		  racers.Append temp		  UpdateStatisticsScreen(racers)//should draw only just that racer.		  		  		  		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub ShowRacerData(event_name as string)		  dim rs, rsracer as RecordSet		  dim event_id as string		  dim racer_ids(-1) as string		  dim name as string		  dim rank as string		  dim average as string		  dim best_lap as string		  dim best_time_string as string		  dim best_time as Double		  dim racers(-1) as raceCar		  dim laps_leading as string		  		  ListBox_ST_Racers.DeleteAllRows//clean it before hand~		  		  event_id = GetEventID(event_name)//we need to know the event_id based from it's event_name		  		  //Make sure we got a valid event id		  if event_id = "" then		    return		  end if		  		  racer_ids = getRacerIDs(event_id)//now we need to know the racers based on the event_id		  		  //Now we have the event_id, and all the racer_ids associated with it, any information from other tables will be simple now.		  		  //NAME		  for i as Integer = 0 to UBound(racer_ids)		    name = getRacerName(racer_ids(i))		    ListBox_ST_Racers.AddRow name		    ListBox_ST_Racers.CellTag(i,0)=racer_ids(i)//store a copy of the ID so we can reference the database later since racer name are NOT unique.		  next		  		  'The list box is now initially filled with the names of all the racers, now in the exact same order based on the racer_ids we will fill in the rest of the		  'list box information		  		  //RANK,AVERAGE,BEST_LAP		  for i as Integer = 0 to UBound(racer_ids)		    rs = G_Database.SQLSelect("SELECT * FROM summary_table WHERE racer_id=='"+racer_ids(i)+"' AND event_id=='"+event_id+"'")		    rank = rs.Field("rank").StringValue		    average = rs.Field("average_time").StringValue		    best_lap =  rs.Field("best_lap").StringValue		    best_time = rs.Field("best_time").DoubleValue		    best_time_string = format(best_time,G_Preferences.lapTimeFormat)		    laps_leading = rs.Field("laps_leading").StringValue		    		    ListBox_ST_Racers.cell(i,1)=rank		    ListBox_ST_Racers.cell(i,4)=best_lap+"/"+best_time_string		    ListBox_ST_Racers.cell(i,5)=average		    ListBox_ST_Racers.cell(i,6)=laps_leading		  next		  		  //Laps and Time		  dim tempCar as raceCar		  for i as integer = 0 to UBound(racer_ids)		    rs = G_Database.SQLSelect("SELECT * FROM laps_table WHERE racer_id=='"+racer_ids(i)+"' AND event_id=='"+event_id+"'")		    rsracer = G_Database.SQLSelect("SELECT * FROM racer_table WHERE racer_id=='"+racer_ids(i)+"'")		    if rs<>nil and rsracer<>nil then		      if rs.RecordCount < 1 then		        //We have no lap data do not display		        return		      end if		      		      tempCar = new raceCar		      while not rs.EOF		        tempCar.lapCounts.Append rs.Field("lap_number").DoubleValue		        tempCar.lapTimes.Append rs.Field("lap_time").DoubleValue		        tempCar.lagTimes.Append rs.Field("lag_time").DoubleValue		        tempCar.lapPlaces.Append rs.Field("lap_place").IntegerValue		        tempCar.timeAtLap.Append rs.Field("time_at_lap").DoubleValue		        tempCar.textColor = G_Preferences.slotColors(i)		        rs.MoveNext		      wend		    else		      MsgBox "Error finding laps table information"		    end if		    //Find the race car name since we need it from the racer_table		    tempCar.theName=getRacerName(racer_ids(i))//gets the name based from the ID passed		    tempCar.theColor=GM_StringToColor(rsracer.Field("color").StringValue)		    		    ListBox_ST_Racers.cell(i,2)=cstr(tempCar.lapCounts(UBound(tempCar.lapCounts)))//actually show that it grabbed the highest values in the Listbox		    ListBox_ST_Racers.cell(i,3)=secondsToTime(tempCar.lapTimes(UBound(tempCar.lapTimes)), G_Preferences.lapTimeFormat)		    		    //Fixme, cheesy way to remove lap 0 from statistics graphs		    //No longer needed, lap 0 is being removed in the global animation method		    //if UBound(tempCar.lapCounts) > 1 then		    //tempCar.lapCounts.Remove(0)		    //tempCar.lapTimes.Remove(0)		    //tempCar.lagTimes.Remove(0)		    //tempCar.timeAtLap.Remove(0)		    //end if		    		    racers.Append tempCar//appends the racer info~		  next		  		  UpdateStatisticsScreen(racers)//with the racer array that we created we can pass that to the statistics animator function and it draws the graph for us.		  		  ListBox_ST_Racers.SortedColumn=1//so it sorts with rank~		  ListBox_ST_Racers.Sort()		  		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub speakRank(timeleft as double)		  //This module is used to speak the current ranks at the interval specified in the perferences		  //Check to see if we are even supposed to run		  if G_Preferences.rankAnnoucements = False then		    return		  end if		  		  		  dim timeleft100 as integer		  dim integertime as integer		  dim temptime as integer		  //Create a static variable so we can store if we have already done this interval		  static lasttime as integer		  		  //Make sure we have a valid interval		  if G_Preferences.rankAnnInterval < 1 then		    return		  end if		  		  //If we missed exit		  if timeleft < 55 then		    return		  end if		  		  		  		  //Only do this work if audio is enabled		  If G_Preferences.useAudio = True then		    		    if G_Preferences.theRaceObject.RaceType <> "Lap" then		      timeleft = G_Preferences.theRaceObject.timeLimit - timeleft		      timeleft100 = (timeleft - 3) * 100		    else		      timeleft100 = (timeleft + 3) * 100		    end if		    		    //We want to work with integers but still have hundreths of a second represented		    //Add one second so that it will trigger 1 second after the time interval, we do this so it will not conflict with the time remaining speech		    		    		    		    integertime = timeleft100/(G_Preferences.rankAnnInterval * 6000)		    temptime = integertime* (G_Preferences.rankAnnInterval * 6000) - timeleft100		    		    select case temptime		      		    case  -10 to 10		      //make sure that the race did not just start		      if timeleft100 <> G_Preferences.theRaceObject.timeLimit * 100 then		        if integertime <> lasttime then		          lasttime = integertime		          robotSpeak(G_Preferences.theRaceObject.LiveRankString, false)		        end if		        		      end if		      		    end select		    		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub speakTimeRemaining(timeleft as double)		  //This module will say the time remaining every 10 minutes until less than 10 minutes remain		  //At less than 10 minutes it makes announcments at 5 minutes, 2 minutes, 1 minute and 30 seconds		  		  		  dim timeleft100 as integer		  dim timeleftstring as string		  dim integertime as integer		  dim temptime as integer		  dim lminremaining as string		  dim lminsremaining as string		  dim lsecondsremaining as string		  dim lhourremaining as string		  dim lhoursremaining as string		  dim lhourand as string		  dim lhoursand as string		  		  lminremaining = " " + G_Preferences.speechText.Value("minuteremaining")		  lminsremaining = " " + G_Preferences.speechText.Value("minutesremaining")		  lsecondsremaining = " " + G_Preferences.speechText.Value("secondsremaining")		  lhourremaining = G_Preferences.speechText.Value("hourremaining") + " "		  lhoursremaining = " " + G_Preferences.speechText.Value("hoursremaining")		  lhourand = " " + G_Preferences.speechText.Value("hourandremaining") + " "		  lhoursand = " " + G_Preferences.speechText.Value("hoursandremaining") + " "		  		  		  		  		  		  timeleft100 = timeleft * 100		  		  //maybe try math of dividing by 60000 and comparing		  integertime = timeleft100/60000		  temptime = integertime*60000 - timeleft100		  		  select case integertime		    		  case 1 to 5		    //Used when 10 to 50 minutes remain		    select case temptime		    case -10 to 10		      timeleftstring = format(timeleft100/6000,"#") + lminsremaining		      temptime = timeleft100/60000		      timeleft100 = temptime*60000		    end select		    		  case 0		    //Used when less than 10 minutes remain		    select case timeleft100		    case 2990 to 3010		      timeleft100 = 3000		      timeleftstring = format(timeleft100/100, "#")  + lsecondsremaining		    case 5990 to 6010		      timeleft100 = 6000		      timeleftstring = lminremaining		    case 11990 to 12010		      timeleft100 = 12000		      timeleftstring = format(timeleft100/6000, "#")  + lminsremaining		    case 29990 to 30010		      timeleft100 = 30000		      timeleftstring = format(timeleft100/6000, "#")  + lminsremaining		      		    end select		    		  case 6		    //Used only when 1 hour remains		    select case temptime		    case -10 to 10		      timeleftstring = lhourremaining		      temptime = timeleft100/60000		      timeleft100 = temptime*60000		    end select		    		  case 7 to 11		    //In here make announcments that include how many hours remain between 1 and 2 hours		    select case temptime		    case -10 to 10		      timeleftstring = lhourand + format(timeleft100/6000 - 60,"#") + lminsremaining		      temptime = timeleft100/60000		      timeleft100 = temptime*60000		    end select		    		  case is > 11		    //Everything else here		    select case temptime		    case -10 to 10		      dim hoursleft as integer		      dim minutesleft as integer		      hoursleft = floor(integertime/6)		      minutesleft = timeleft100/6000 - hoursleft * 60		      		      //This logic makes it so it will not say the minutes remaining after it says how many hours are left if it is just on the hour		      if minutesleft = 0 then		        timeleftstring = format(hoursleft,"#") + lhoursremaining		      else		        timeleftstring = format(hoursleft,"#") + lhoursand + format(timeleft100/6000  - hoursleft * 60,"#") + lminsremaining		      end if		      temptime = timeleft100/60000		      timeleft100 = temptime*60000		    end select		    		  end select		  		  		  //make sure that the race did not just start		  if timeleft100 <> G_Preferences.theRaceObject.timeLimit * 100 then		    //make sure we have not already said this time remaining		    if timeleft100 <> M_Announcedtime and timeleftstring <> "" then		      M_Announcedtime = timeleft100		      robotSpeak(timeleftstring, true)		    end if		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub startOrStopRace(eventItemId as Integer = - 1)		  dim activeIndexes(-1) as Integer		  dim i as Integer		  		  static processing as boolean = false		  		  		  //Since this routine can be called from multiple locations we want to prevent possible double processing		  if processing = false then		    processing = true		    		    		    //Check to make sure some racers have been created		    if UBound(G_Preferences.theRaceObject.racers) < 0 then		      MsgBox t_createracersfirst		      processing = false		      return		    end if		    		    		    activeIndexes = FindActiveIndexes()//finds the active Indexes like the name implies from M_Slots		    		    if G_Preferences.theRaceObject.state="stopped" then		      //initialize hardware		      if G_Preferences.hardwareType = "CoreSpeedway" or G_Preferences.hardwareType = "CoreSpeedway i166" then		        		        serial1.Write(ChrB(&h01) + ChrB(&h0A) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&hF4) + ChrB(&h00) + ChrB(&hFF) + ChrB(&h00))		        SerialLapCounters2.initialize		      else		        SerialLapCounters1.initialize		        SerialLapCounters2.initialize		      end if		      		      //If they happen to leave the event name blank make sure to do an auto event name		      if G_Preferences.theRaceObject.eventName = "" then		        G_Preferences.write("autoNameEvent",true)		      end if		      		      if G_Preferences.readB("autoNameEvent")=true then		        AlterEventName()		      end if		      		      //If countdown is 10 seconds or greater play the prepare to sound		      if G_Preferences.theRaceObject.countDownTime > 7 then		        countDownCanvas.Backdrop=Countdown_10		        if G_Preferences.useAudio=true then		          robotspeak(G_Preferences.speechText.Value("preparetorace"),true)		        end if		      end if		      		      //Make sure to pass the event item id if it was used so we know what managed event to link to		      G_Preferences.theRaceObject.StartRace(eventItemId)		      rowSpacing = G_Preferences.rowSpacing		      ChangeRacerRowSpacing(rowSpacing)		      		    else		      if G_Preferences.theRaceObject.state="paused" then		        		        //reinitialize hardware		        if G_Preferences.hardwareType = "CoreSpeedway" or G_Preferences.hardwareType = "CoreSpeedway i166" then		          		          serial1.Write(ChrB(&h01) + ChrB(&h0A) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&hF4) + ChrB(&h00) + ChrB(&hFF) + ChrB(&h00))		          SerialLapCounters2.initialize		        else		          SerialLapCounters1.initialize		          SerialLapCounters2.initialize		        end if		        		        		        if countDownTimerActive=false then		          M_StartTime = Microseconds		          G_Preferences.theRaceObject.tempTimeLimit=G_Preferences.theRaceObject.timeRemaining		          G_Preferences.theRaceObject.tempTimeElapsed=G_Preferences.theRaceObject.timeElapsed		          		          //The racerCars and there lastTimeChecked Values Need to be adjusted for the change since the pause		          dim differenceTime as double		          for i=0 to UBound(activeIndexes)		            differenceTime = (M_Slots(activeIndexes(i)).tempTimeSinceLastCheck - M_Slots(activeIndexes(i)).TimeSinceLastCheck)		            M_Slots(activeIndexes(i)).timeSinceLastCheck=Microseconds - differenceTime//Time Paused - Time known before last = difference to use against 'new ticks' to simulate the same time.		          next		          		          raceTimer.Mode=2//start the timer back up~		          SerialLapCounters1.lightState("start")		        else		          //Set the interval just in case		          countDownTimer.Period = 1000		          countDownTimer.Mode=2		        end if		        		        M_GamePaused=false		        startStopButton.Caption=t_pause		        G_Preferences.theRaceObject.state="started"		        stopRaceButton.Enabled = true		      else		        if countDownTimerActive=false then		          raceTimer.Mode=0		        else		          countDownTimer.Mode=0//stop it for now		        end if		        		        M_PauseTimePoint=Microseconds		        for i=0 to UBound(activeIndexes)//store the point in time we paused it at.		          M_Slots(activeIndexes(i)).tempTimeSinceLastCheck=M_PauseTimePoint		        next		        		        M_GamePaused=true		        startStopButton.Caption=t_resume		        G_Preferences.theRaceObject.state="paused"		        stopRaceButton.Enabled = false		        SerialLapCounters1.lightState("stop")		        		        		      end if		    end if		    //end processing		    processing = false		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub UpdateStatisticsScreen(racers() as racecar)		  if mainWindow.M_graphTypeSelected="Laps Versus Time" then		    GM_AnimateStatisticsCanvas(racers,t_lapsversustime,t_time + " (" + t_seconds + ")",t_laps,mainWindow.statisticsCanvas,"Time_Graph")		  elseif mainWindow.M_graphTypeSelected="Laps Versus Lag" then		    GM_AnimateStatisticsCanvas(racers,t_lapsversuslag,t_laps,t_lag + " (" + t_seconds + ")",mainWindow.statisticsCanvas,"Lag_Graph")		  elseif mainWindow.M_graphTypeSelected="Time Per Lap" then		    GM_AnimateStatisticsCanvas(racers,t_timeperlap,t_laps,t_time + " (" + t_seconds + ")",mainWindow.statisticsCanvas,"Time_Per_Lap")		  elseif mainWindow.M_graphTypeSelected="Rank Versus Time" then		    GM_AnimateStatisticsCanvas(racers,t_rankversustime,t_time + " (" + t_seconds + ")",t_rank,mainWindow.statisticsCanvas, "Rank_Time")		  else		    MsgBox "Graph type for mainWindow not recognized."		  end if		  		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub viewReport()		  //This function will make an xml file in the reports section with the neccesary parts for the report in it's own folder and special name.		  		  dim event_name as string = WhatEventIsSelected()		  dim event_id as string = getEventID(event_name)		  dim race_type as string		  dim num_laps as string		  dim time_limit as string		  dim round_type as string		  dim racer_id as string		  dim lap_time as string		  dim lap_number as string		  dim lap_rank as string		  dim time_at_lap as string		  dim rs as RecordSet		  dim oFilestream as TextOutputStream		  dim iFilestream as TextInputStream		  dim pi as XmlProcessingInstruction		  		  rs = G_Database.SQLSelect("SELECT * FROM race_table WHERE event_name=='"+event_name+"'")		  if rs<>nil then		    		    select case rs.Field("race_type").StringValue		    case "Timed"		      race_type = t_timed		    case "Lap"		      race_type = t_lap		    case "Lap and Timed"		      race_type = t_lapandtimed		    case "Staggered Time"		      race_type = t_staggeredtime		    case "Free Play"		      race_type = t_freeplay		    case "Drag"		      race_type = t_drag		    end select		    		    num_laps = rs.Field("num_laps").StringValue		    time_limit = rs.Field("time_limit").StringValue		    		    select case rs.Field("round_type").StringValue		    case "Practice"		      round_type = t_practice		    case "Heat"		      round_type = t_heat		    case "Qualifier"		      round_type = t_qualifier		    case "Main"		      round_type = t_main		    end select		    		  else		    MsgBox "Error getting database info for event while trying to make report."+EndOfLine+EndOfLine+G_Database.ErrorMessage		  end if		  		  		  		  dim new_event_name as string = ReplaceAll(event_name,":","-")//this will be used down below later.		  dim f as FolderItem =  GetFolderItem("reports").Child(new_event_name)//because on mac you can't use the colon~		  f.CreateAsFolder		  		  dim xslFile as FolderItem = GetFolderItem("reports").Child("race.xsl")		  dim indentXslFile as FolderItem = GetFolderItem("reports").Child("indent.xsl")		  		  iFilestream = TextInputStream.Open(xslFile)		  dim xslText as string = iFilestream.ReadAll		  		  iFilestream = TextInputStream.Open(indentXslFile)		  dim xslIndentText as string = iFilestream.ReadAll		  		  if f.Exists=true and f<>nil then		    dim xml as new XmlDocument//creates an empty xml document~		    pi = xml.CreateProcessingInstruction("xml-stylesheet", "type='text/xsl' href='race.xsl'") //adds sytlesheet info so browsers can render XML directly		    xml.AppendChild pi		    		    //copy xsl file to report directory		    xslFile.CopyFileTo f		    		    //All the nodes involved are below~		    dim rootNode,clubNode,eventNode,summaryNode,racerNode as XmlNode		    dim racerNameNode,positionNode,lapsNode,timeNode,averageNode,bestNode,lapsLeadingNode as XmlNode		    dim eventNameNode,typeNode,lapLimitNode,timeLimitNode,roundTypeNode as XmlNode		    dim clubNameNode,clubImageNode as XmlNode		    dim lapInfoNode,lapNumberNode,lapTimeNode as XmlNode		    dim lapRankNode,timeAtLapNode as XmlNode		    dim URIattr as XmlAttribute		    dim textNode, textPosition, textLaps, textTime, textAverage, textBest, textLapsLeading, textName, textLapTimes, textLap, textRaceResults as XmlNode		    dim clubLogoNode as XmlNode		    		    rootNode = xml.AppendChild(xml.CreateElement("race"))		    		    clubNode = rootNode.AppendChild(xml.CreateElement("club"))		    clubNameNode = clubNode.AppendChild(xml.CreateElement("name"))		    clubNameNode.AppendChild(xml.CreateTextNode(G_Preferences.readS("clubName")))//stores the club name to the file~		    		    clubImageNode = clubNode.AppendChild(xml.CreateElement("image"))		    URIattr = xml.CreateAttribute("URI")		    URIattr.value =  G_Preferences.clubLogoPath		    clubImageNode.SetAttributeNode(URIattr)		    		    		    //We will insert the logo directly into the xml so that the xml report is complete without external files		    //Method does not work with Outlook or GMAIL		    //if G_Preferences.clubLogo <> Nil then		    //dim rawPic as string		    //dim encodedPic as string		    //rawPic = G_Preferences.clubLogo.GetData(Picture.FormatJPEG,99)		    //encodedPic = EncodeBase64(rawPic,0)		    //clubLogoNode = clubNode.AppendChild(xml.CreateElement("clubLogo"))		    //clubLogoNode.AppendChild(xml.CreateTextNode(encodedPic))		    //end if		    		    		    eventNode = rootNode.AppendChild(xml.CreateElement("event"))		    eventNameNode = eventNode.AppendChild(xml.CreateElement("name"))		    eventNameNode.AppendChild(xml.CreateTextNode(event_name))		    typeNode = eventNode.AppendChild(xml.CreateElement("type"))		    typeNode.AppendChild(xml.CreateTextNode(race_type))		    lapLimitNode = eventNode.AppendChild(xml.CreateElement("lap_limit"))		    lapLimitNode.AppendChild(xml.CreateTextNode(num_laps))		    timeLimitNode = eventNode.AppendChild(xml.CreateElement("time_limit"))		    timeLimitNode.AppendChild(xml.CreateTextNode(time_limit))		    roundTypeNode = eventNode.AppendChild(xml.CreateElement("round_type"))		    roundTypeNode.AppendChild(xml.CreateTextNode(round_type))		    		    //Add entries to be used for translation		    textNode = rootNode.AppendChild(xml.CreateElement("translations"))		    textPosition = textNode.AppendChild(xml.CreateElement("position"))		    textPosition.AppendChild(xml.CreateTextNode(t_position))		    textLaps = textNode.AppendChild(xml.CreateElement("laps"))		    textLaps.AppendChild(xml.CreateTextNode(t_laps))		    textTime = textNode.AppendChild(xml.CreateElement("time"))		    textTime.AppendChild(xml.CreateTextNode(t_time))		    textAverage = textNode.AppendChild(xml.CreateElement("average"))		    textAverage.AppendChild(xml.CreateTextNode(t_average))		    textBest = textNode.AppendChild(xml.CreateElement("best"))		    textBest.AppendChild(xml.CreateTextNode(t_best))		    textLapsLeading = textNode.AppendChild(xml.CreateElement("laps_leading"))		    textLapsLeading.AppendChild(xml.CreateTextNode(t_lapsleading))		    textName = textNode.AppendChild(xml.CreateElement("name"))		    textName.AppendChild(xml.CreateTextNode(t_name))		    textLapTimes = textNode.AppendChild(xml.CreateElement("lap_times"))		    textLapTimes.AppendChild(xml.CreateTextNode(t_laptimes))		    textLap = textNode.AppendChild(xml.CreateElement("lap"))		    textLap.AppendChild(xml.CreateTextNode(t_lap))		    textRaceResults = textNode.AppendChild(xml.CreateElement("race_results"))		    textRaceResults.AppendChild(xml.CreateTextNode(t_raceresults))		    		    summaryNode = rootNode.AppendChild(xml.CreateElement("summary"))		    		    for i as Integer = 0 to ListBox_ST_Racers.ListCount-1		      racerNode = summaryNode.AppendChild(xml.CreateElement("racer"))		      racerNameNode = racerNode.AppendChild(xml.CreateElement("name"))		      racerNameNode.AppendChild(xml.CreateTextNode(ListBox_ST_Racers.cell(i,0)))		      positionNode = racerNode.AppendChild(xml.CreateElement("position"))		      positionNode.AppendChild(xml.CreateTextNode(ListBox_ST_Racers.cell(i,1)))		      lapsNode = racerNode.AppendChild(xml.CreateElement("laps"))		      lapsNode.AppendChild(xml.CreateTextNode(ListBox_ST_Racers.cell(i,2)))		      timeNode = racerNode.AppendChild(xml.CreateElement("time"))		      timeNode.AppendChild(xml.CreateTextNode(ListBox_ST_Racers.cell(i,3)))		      averageNode = racerNode.AppendChild(xml.CreateElement("average"))		      averageNode.AppendChild(xml.CreateTextNode(ListBox_ST_Racers.cell(i,5)))		      bestNode = racerNode.AppendChild(xml.CreateElement("best"))		      bestNode.AppendChild(xml.CreateTextNode(ListBox_ST_Racers.cell(i,4)))		      lapsLeadingNode = racerNode.AppendChild(xml.CreateElement("lapsleading"))		      lapsLeadingNode.AppendChild(xml.CreateTextNode(ListBox_ST_Racers.cell(i,6)))		      		      racer_id = ListBox_ST_Racers.CellTag(i,0)//this should hold the racerID		      rs = G_Database.SQLSelect("SELECT * FROM laps_table WHERE event_id=='"+event_id+"' AND racer_id=='"+racer_id+"';")		      if rs<>nil then		        while not rs.EOF		          lap_Time = rs.Field("time_at_lap").StringValue		          lap_Number = rs.Field("lap_number").StringValue		          time_at_lap = rs.Field("lap_time").StringValue		          lap_rank = rs.Field("lap_place").StringValue		          		          if cdbl(lap_number)<>0 then//no need to show the '0th' lap, nothing there, it's only there for graphing purposes.		            lapInfoNode = racerNode.AppendChild(xml.CreateElement("lapinfo"))//creates the lap info elements		            lapNumberNode = lapInfoNode.AppendChild(xml.CreateElement("lapnumber"))		            lapNumberNode.AppendChild(xml.CreateTextNode(lap_number))		            lapTimeNode = lapInfoNode.AppendChild(xml.CreateElement("laptime"))		            lapTimeNode.AppendChild(xml.CreateTextNode(lap_time))		            timeAtLapNode = lapInfoNode.AppendChild(xml.CreateElement("timeatlap"))		            timeAtLapNode.AppendChild(xml.CreateTextNode(time_at_lap))		            lapRankNode = lapInfoNode.AppendChild(xml.CreateElement("laprank"))		            lapRankNode.AppendChild(Xml.CreateTextNode(lap_rank))		            		          end if		          rs.MoveNext		        wend		      else		        MsgBox "Error getting lap_table info from database"		      end if		      		    next		    		    dim transformedXml as string		    		    transformedXml = xml.Transform(xslText)//we now the fancy form of the xml ready to save as html~		    		    dim outputRaceTransformFile as FolderItem = GetFolderItem("reports").Child(new_event_name).Child("race.html")		    oFilestream = TextOutputStream.Create(outputRaceTransformFile)		    oFilestream.Write(transformedXml)		    		    //Switch to copying file instead of reading into picture object and writing to file		    //dim logoFile as FolderItem = GetFolderItem("reports").Child(new_event_name).Child(G_Preferences.clubLogoPath)		    //logoFile.SaveAsPicture(G_Preferences.clubLogo)//saves a copy of the club logo to the report folder for that race so the xsl works~		    dim clublogodest as FolderItem = GetFolderItem("reports").Child(new_event_name)		    dim clublogosource as FolderItem = GetFolderItem("club_logo").Child(G_Preferences.clubLogoPath)		    clublogosource.CopyFileTo(clublogodest)		    		    dim indentXmlFile as FolderItem = GetFolderItem("reports").Child(new_event_name).Child("race.xml")		    //Fixme		    //Do not indent file at this time as IE does not like the results		    //dim indentedXml as string = xml.Transform(xslIndentText)		    dim indentedXml as string = xml.Transform(xslIndentText)		    oFilestream = TextOutputStream.Create(indentXmlFile)		    oFilestream.Write(indentedXml)//this is purely so we have a copy of the xml, it's tecnically not needed in the folder		    		    oFilestream.Close//now where were done with it		    iFilestream.Close//were done with this too		    		    //Now lets launch the report in there browser so they can see!		    //outputRaceTransformFile.Launch()		    If TargetWin32 then		      ShowURL(outputRaceTransformFile.URLPath)		    elseif TargetMacOS then		      ShowURL(outputRaceTransformFile.URLPath)		    elseif TargetLinux then		      dim s as shell		      s = new shell		      s.execute "xdg-open " + outputRaceTransformFile.URLPath		    end if		    		  else		    MsgBox "Error getting folder item!"		  end if		  		Exception XmlException		  MsgBox "An XML Error Occured, the document was not transformed correctly"		  		  		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function WhatEventIsSelected() As string		  dim row as Integer=0		  dim pass as Boolean = true		  		  for i as Integer=0 to ListBox_ST_Events.ListCount		    if ListBox_ST_Events.Selected(i)=true then		      row=i		      pass=true		      exit		    else		      pass=false		    end if		  next		  if pass=true then		    return ListBox_ST_Events.cell(row,0)		  else		    //in the case that nothing is selected just use the first row and make it selected, but only if there is at least 'that element in the listbox'		    if ListBox_ST_Events.ListCount>0 then		      ListBox_ST_Events.Selected(0)=true		      return ListBox_ST_Events.cell(0,0)		    end if		  end if		  ListBox_ST_Events.SetFocus		  //I need to make it the showRacerData function update mainWindow animateRaceStatistics() function accordingly~		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function WhatRacerIsSelected() As string		  dim row as Integer=0		  dim pass as Boolean = true		  		  for i as Integer=0 to ListBox_ST_Racers.ListCount		    if ListBox_ST_Racers.Selected(i)=true then		      row=i		      pass=true		      exit		    else		      pass=false		    end if		  next		  if pass=true then		    return ListBox_ST_Racers.cell(row,0)		  else		    //in the case that nothing is selected just use the first row and make it selected, but only if there is at least 'that element in the listbox'		    if ListBox_ST_Racers.ListCount>0 then		      ListBox_ST_Racers.Selected(0)=true		      return ListBox_ST_Racers.cell(0,0)		    end if		  end if		  ListBox_ST_Racers.SetFocus		End Function	#tag EndMethod	#tag Note, Name = LICENSE		Copyright 2008 Jeremy Auten				This file is part of Flip Side Racing Software.				Flip Side Racing Software is free software: you can redistribute it and/or modify		it under the terms of the GNU General Public License as published by		the Free Software Foundation, either version 3 of the License, or		(at your option) any later version.				Flip Side Racing Software is distributed in the hope that it will be useful,		but WITHOUT ANY WARRANTY; without even the implied warranty of		MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the		GNU General Public License for more details.				You should have received a copy of the GNU General Public License		along with Flip Side Racing Software.  If not, see <http://www.gnu.org/licenses/>.	#tag EndNote	#tag Property, Flags = &h0		assembledHex() As String	#tag EndProperty	#tag Property, Flags = &h0		countDownTimerActive As boolean	#tag EndProperty	#tag Property, Flags = &h0		Countdown_0 As Picture	#tag EndProperty	#tag Property, Flags = &h0		Countdo