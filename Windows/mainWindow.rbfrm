  		  subject = "[FSRContact] " + subject		  		  if trim(TextField_PE_YourName.Text) = "" then		    name = "No Name"		  else		    name = TextField_PE_YourName.Text		  end if		  		  		  if trim(TextArea_PE_YourMessage.Text) = "" then		    message = "No Message"		  else		    message = TextArea_PE_YourMessage.Text		  end if		  		  message = message + EndOfLine + EndOfLine		  message = message + "System Info" + EndOfLine		  message = message + "name: " + name + EndOfLine		  message = message + "version: "+version+EndOfLine		  message = message + "os: "+osString+EndOfLine		  message = message + "clubName: "+G_Preferences.readS("clubName")+EndOfLine		  message = message + "lapCounter: "+G_Preferences.hardwareType+EndOfLine		  message = message + "racers: "+racersString+EndOfLine		  message = message + "races: "+racesString		  		  		  //formData.Value("key")="thisisakeyweee"		  formData.Value("name")=name		  formData.Value("email")=email		  formData.Value("subject")=subject		  formData.Value("message")=message		  formData.Value("_formname")="messages/create"		  		  //Pop up a message saying we attempting to send		  dim tempWindow as new messageWindow		  tempWindow.timeLimit = 1		  tempWindow.message = t_sendingmessage		  		  tempSocket.SetFormData formData		  tempSocket.Post "http://web2py.flipsidehobbies.com/fsrclients/messages/post/"		  		  if tempSocket.LastErrorCode <> 0 then		    tempWindow.Close		    MsgBox "Error sending message"		  end if		  		  		End Sub	#tag EndEvent#tag EndEvents#tag Events stopRaceButton	#tag Event		Sub Action()		  EndTheRace()		  		End Sub	#tag EndEvent#tag EndEvents#tag Events lookAtLiveStatisticsButton	#tag Event		Sub Action()		  PagePanel1.Value=5		End Sub	#tag EndEvent#tag EndEvents#tag Events Rectangle_ER_Color	#tag Event		Function MouseDown(X As Integer, Y As Integer) As Boolean		  Dim c  as Color		  Dim b as Boolean		  		  c = me.FillColor		  b = SelectColor(c,t_se