tus = SerialLapCounters1.selfOpen(System.SerialPort(i), G_Preferences.hardwareType)		          if status = false then		            //Failed to open		            MsgBox t_couldnotopen + " " + System.SerialPort(i).Name		            return		          end if		          selectDevicePopUpMenu.ListIndex=i//Set the popup to the configured device		          //no need to continue loop		          return		        end if		      next		    end if		    		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events openButton	#tag Event		Sub Action()		  dim count as integer		  dim portNum as integer		  dim i as integer		  		  //Make sure we can find a port that matches the selected name		  count = System.SerialPortCount		  portNum = -1		  		  for i = 0 to count - 1		    if selectDevicePopUpMenu.Text = System.SerialPort(i).Name then		      portNum = i		    end if		  next		  		  		  //If we did not find a match, or if no port is selected then bail		  if selectDevicePopUpMenu.ListIndex = -1 or portNum = -1 then		    MsgBox t_couldnotopen		    return		  end if		  		  		  dim status as boolean		  		  status = false		  SerialLapCounters1.Close		  Serial1.Close		  		  if selectDevicePopUpMenu.Text <> "none" and selectDevicePopUpMenu.ListIndex > -1 then		    //We have a COM Port		    		    if G_Preferences.hardwareType = "CoreSpeedway" or G_Preferences.hardwareType = "CoreSpeedway i166" then		      		      SerialLapCounters1.hardwaretype = G_Preferences.hardwareType		      Serial1.SerialPort = System.SerialPort( portNum )		      Serial1.Baud = 57600		      		      if not Serial1.Open then		        ' If we couldn't open the port, then bail out		        MsgBox t_couldnotopen + " " + System.SerialPort( portNum ).Name		        return		      else		        dim tempWindow as new messageWindow		        tempWindow.message = t_connectionestablished		        tempWindow.timeLimit = 1		        		        G_Preferences.write("comPort",System.SerialPort( portNum ).Name)		        		        		        //Get reader version		        //This is needed to detect different types of Core hardware, the response will tell us what we are dealing with		        Serial1.Write(ChrB(&h01) + ChrB(&h09) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&h00) + ChrB(&hF0) + ChrB(&hF8) + ChrB(&h07))		        		      end if		      		    else		      //Use the generic class from the serial lap counters		      		      status = SerialLapCounters1.selfOpen(System.SerialPort(portNum), G_Preferences.hardwareType)		      if status = false then		        MsgBox t_couldnotopen + " " + System.SerialPort( portNum ).Name		        return		      else		        dim tempWindow as new messageWindow		        tempWindow.message = t_connectionestablished		        tempWindow.timeLimit = 1		        		        G_Preferences.write("comPort",System.SerialPort( portNum ).Name)		        		      end if		      		    end if		    		    		  else		    		    G_Preferences.write("comPort","none")		    		    		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events PopupMenu1_GC_HWType	#tag Event		Sub Change()		  if windowOpened = True then		    G_Preferences.hardwareType=me.Text		    G_Preferences.write("hardwareType",me.Text)		    		    //Reset accuracy setting		    PopupMenu_GC_HWAccuracy.ListIndex = 0		    dim tempSerial as new SerialLapCounters		    tempSerial.hardwaretype = G_Preferences.hardwareType		    G_Preferences.lapTimeFormat = tempSerial.lapTimeFormat		    		    if G_Preferences.hardwareType = "CoreSpeedway" or G_Preferences.hardwareType = "CoreSpeedway i166" then		      ScanButtonsVisible(True)		    else		      ScanButtonsVisible(False)		    end if		  end if		  		End Sub	#tag EndEvent	#tag Event		Sub Open()		  dim i as integer		  		  for i = 0 to me.ListCount - 1		    if me.List(i) = G_Preferences.hardwareType then		      me.ListIndex = i		      exit		    end if		  next		End Sub	#tag End