ame as String)		  dim i as integer		  //To prevent some unneccessary saves		  loadingEventSettings = true		  		  //Display this events settings		  PopupMenu_EM_QSort.ListIndex = fsEventManager.qSortingMethod		  EditField_EM_qRounds.Text = str(fsEventManager.qRounds)		  EditField_EM_qNumOfDrivers.Text = str(fsEventManager.qNumOfDrivers)		  		  //Match up the qualifier race profile		  for i = 0 to PopupMenu_EM_qRaceProfile.ListCount - 1		    if PopupMenu_EM_qRaceProfile.List(i) = fsEventManager.qRaceProfile then		      PopupMenu_EM_qRaceProfile.ListIndex = i		    end if		  next		  		  EditField_EM_mBumpUps.Text = str(fsEventManager.mBumpUps)		  EditField_EM_mNumOfDrivers.Text = str(fsEventManager.mNumOfDrivers)		  		  //Match up the main race profile		  for i = 0 to PopupMenu_EM_mRaceProfile.ListCount - 1		    if PopupMenu_EM_mRaceProfile.List(i) = fsEventManager.mRaceProfile then		      PopupMenu_EM_mRaceProfile.ListIndex = i		    end if		  next		  		  //Set loading to false now that we are done		  loadingEventSettings = false		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub initialize(tempEM as eventManager)		  //This method creates objects needed for this class		  fsEventManager = tempEM		  		End Sub	#tag EndMethod	#tag Property, Flags = &h0		fsEventManager As eventManager	#tag EndProperty	#tag Property, Flags = &h0		loadingEventSettings As Boolean = False	#tag EndProperty#tag EndWindowCode#tag Events PushButton_EM_CreateEvent	#tag Event		Sub Action()		  //Create new event		  if PopupMenu_EM_Events.Text <> "" then		    dim status as boolean		    status = fsEventManager.createEvent(PopupMenu_EM_Events.Text)		    if status = False then		      MsgBox "Error creating event"		    end if		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events PushButton_EM_DeleteEvent	#tag Event		Sub Action()		  dim success as boolean		  		  success = fsEventManager.deleteEvent(PopupMenu_EM_Events.Text)		  		